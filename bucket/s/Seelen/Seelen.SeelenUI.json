{
    "version": "2.4.11",
    "description": "适用于 Windows 的完全可定制的桌面环境 | Fully Customizable Desktop Environment for Windows",
    "homepage": "https://github.com/eythaann/Seelen-UI",
    "license": {
        "identifier": "AGPL-3.0-only"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/eythaann/Seelen-UI/releases/download/v2.4.11/Seelen.UI_2.4.11.0_x64.Msix",
            "hash": "a0061e4ad44a02e5f0603ef2695c0f130bf45746e7f0d89da065a51c9c3ec7ad"
        }
    },
    "pre_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install",
        "A-Add-MsixPackage 'Seelen.SeelenUI_p6yyn03m1894e'",
        "A-New-LinkDirectory \"$env:AppData\\com.seelen.seelen-ui\""
    ],
    "post_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Stop-Process",
        "A-Remove-MsixPackage",
        "A-Remove-Link",
        "A-Remove-TempData \"$env:LocalAppData\\com.seelen.seelen-ui\""
    ],
    "post_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "$res, $out = A-Get-InstallerInfoFromWinget 'Seelen.SeelenUI' 'msix'",
            "$out"
        ],
        "regex": "(?:.*ver:(?<version>[^;]+))(?:.*x64:(?<x64>[^;]+))?(?:.*x86:(?<x86>[^;]+))?(?:.*arm64:(?<arm64>[^;]+))?"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "$matchX64",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.x64_machine.InstallerSha256"
                }
            }
        }
    }
}
