{
    "version": "4.47.69",
    "description": "一个团队沟通和协作工具 | A team communication and collaboration tool.",
    "homepage": "https://www.slack.com/",
    "license": {
        "identifier": "Freemium"
    },
    "architecture": {
        "64bit": {
            "url": "https://downloads.slack-edge.com/desktop-releases/windows/x64/4.47.69/Slack.msix",
            "hash": "5dad0dd8fddb79d068e580e5b2bfd564fc2858c983b17125a70378f727ec3055"
        },
        "arm64": {
            "url": "https://downloads.slack-edge.com/desktop-releases/windows/arm64/4.47.69/Slack.msix",
            "hash": "318b47fd229684511b86a16c4f873630e0fc2623ac7d2f8b33f105862f20baa3"
        }
    },
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install",
        "A-Add-MsixPackage 'com.tinyspeck.slackdesktop_8yrtsj140pw4g'",
        "A-New-LinkDirectory \"$env:LocalAppData\\Packages\\com.tinyspeck.slackdesktop_8yrtsj140pw4g\\LocalCache\\Roaming\\Slack\""
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Stop-Process",
        "A-Remove-Link",
        "A-Remove-MsixPackage"
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "$InstallerInfo = A-Get-InstallerInfoFromWinget 'SlackTechnologies.Slack' 'msix'",
            "\"ver:$($InstallerInfo.PackageVersion);\"",
            "\"x64:$($InstallerInfo.x64_machine.InstallerUrl);\"",
            "\"x86:$($InstallerInfo.x86_machine.InstallerUrl);\"",
            "\"arm64:$($InstallerInfo.arm64_machine.InstallerUrl);\""
        ],
        "regex": "ver:(?<version>[^;]+)(?:; x64:(?<x64>[^;]*))?(?:; x86:(?<x86>[^;]*))?(?:; arm64:(?<arm64>[^;]*))?"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "$matchX64",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.x64_machine.InstallerSha256"
                }
            },
            "arm64": {
                "url": "$matchArm64",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.arm64_machine.InstallerSha256"
                }
            }
        }
    }
}
