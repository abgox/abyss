{
    "version": "1.1303.0",
    "description": "Snyk CLI 会扫描并监控你的项目，以查找安全漏洞 | Snyk CLI scans and monitors your projects for security vulnerabilities",
    "homepage": "https://snyk.io/",
    "license": {
        "identifier": "Apache-2.0"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/snyk/cli/releases/download/v1.1303.0/snyk-win.exe",
            "hash": "9ec3551c9dc2b6928b016e730f352cb63e69a238063b9113f0016f050fe212ee"
        }
    },
    "bin": [
        [
            "snyk-win.exe",
            "snyk"
        ]
    ],
    "pre_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install"
    ],
    "post_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Stop-Process"
    ],
    "post_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "A-Get-VersionFromGithubAPI"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/snyk/cli/releases/download/v$version/snyk-win.exe"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
