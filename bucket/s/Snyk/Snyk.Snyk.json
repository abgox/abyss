{
    "version": "1.1302.1",
    "description": "Snyk CLI 会扫描并监控你的项目，以查找安全漏洞 | Snyk CLI scans and monitors your projects for security vulnerabilities.",
    "homepage": "https://snyk.io/",
    "license": {
        "identifier": "Apache-2.0"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/snyk/cli/releases/download/v1.1302.1/snyk-win.exe",
            "hash": "67cbb69da0ff610c19de925d5dee55b3c1905a4b94ef4ce83e2ad179dfcde0e1"
        }
    },
    "bin": [
        [
            "snyk-win.exe",
            "snyk"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Stop-Process"
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "A-Get-VersionFromGithubAPI"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/snyk/cli/releases/download/v$version/snyk-win.exe"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
