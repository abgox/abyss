{
    "version": "pending",
    "description": "QQ浏览器 - 腾讯官方出品，基于 Chromium 内核开发，启动速度、打开网页速度更快的浏览器 | Tencent's official Chromium-based browser with faster startup and web page opening speed.",
    "homepage": "https://browser.qq.com/",
    "license": {
        "identifier": "Unknown"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/abgox/abyss/releases/download/empty/empty",
            "hash": "36a9e7f1c95b82ffb99743e0c5c4ce95d83c9a430aac59f84ef3cbfab6145068"
        }
    },
    "shortcuts": [
        [
            "app\\QQBrowser.exe",
            "QQ浏览器"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install",
        "A-Deny-Manifest",
        "A-New-LinkDirectory \"$env:AppData\\Tencent\\QQBrowser\",\"$env:LocalAppData\\Tencent\\QQBrowser\"",
        "A-Install-App -Uninstaller 'xxx'"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Stop-Process",
        "A-Remove-Link",
        "A-Uninstall-App"
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "$InstallerInfo = A-Get-InstallerInfoFromWinget 'Tencent.QQBrowser' 'exe'",
            "\"ver:$($InstallerInfo.PackageVersion);\"",
            "\"x64:$($InstallerInfo.x64_machine.InstallerUrl);\"",
            "\"x86:$($InstallerInfo.x86_machine.InstallerUrl);\"",
            "\"arm64:$($InstallerInfo.arm64_machine.InstallerUrl);\""
        ],
        "regex": "ver:(?<version>[^;]+)(?:; x64:(?<x64>[^;]*))?(?:; x86:(?<x86>[^;]*))?(?:; arm64:(?<arm64>[^;]*))?",
        "replace": "pending"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "$matchX86",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.x86_machine.InstallerSha256"
                }
            }
        }
    }
}
