    "homepage": "https://www.codebuddy.ai/",
    "license": {
        "identifier": "Unknown"
    },
    "architecture": {
        "64bit": {
            "url": "https://codebuddy-1328495429.cos.accelerate.myqcloud.com/aiide/win32-x64-user/CodeBuddy-win32-x64-user-4.2.3.16929265-8da5bbfef2-a50b9bce.exe",
            "hash": "7368d18b135ed9859121793e1da52104e377bf5fc17e60d1e719f99260eaf10a"
        }
    },
    "env_add_path": "app\\bin",
    "shortcuts": [
        [
            "app\\CodeBuddy.exe",
            "CodeBuddy"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Install",
        "A-New-LinkDirectory \"$env:AppData\\CodeBuddy\",\"$env:UserProfile\\.codebuddy\"",
        "A-Install-Exe inno"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Uninstall",
        "A-Stop-Process",
        "A-Remove-Link",
        "A-Uninstall-Exe"
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "url": "https://www.codebuddy.ai/v2/update?platform=ide-win32-x64-user",
        "jsonpath": "$.url",
        "regex": "(?<url>.*?-(?<version>[\\d.]+)-.*\\.*)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "$matchUrl"
            }
        },
        "hash": {
            "url": "https://www.codebuddy.ai/v2/update?platform=ide-win32-x64-user",
            "jsonpath": "$.sha256hash"
        }
    }
}
