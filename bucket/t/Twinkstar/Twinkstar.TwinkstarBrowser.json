{
    "version": "10.13.2000.2601",
    "description": "星愿浏览器，一款方便，可靠，简洁的浏览器 | Twinkstar Browser is a convenient, reliable and simple browser",
    "homepage": "https://www.twinkstar.com/",
    "license": {
        "identifier": "Freeware"
    },
    "notes": [
        "The installation directory: $env:ProgramFiles\\Twinkstar Browser",
        "Refer to: https://abyss.abgox.com/faq/external-installation-directory"
    ],
    "notes_cn": [
        "安装目录: $env:ProgramFiles\\Twinkstar Browser",
        "参考: https://abyss.abgox.com/faq/external-installation-directory"
    ],
    "architecture": {
        "64bit": {
            "url": "https://d1.twinkstar.com/win/Twinkstar_v10.13.2000.2601_Release.exe",
            "hash": "65bf1d3f23d71a837fcb6c5c6082baf005d45453eecb7454e3d41caeb28cebf7"
        }
    },
    "pre_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install",
        "A-Require-Admin",
        "A-New-LinkDirectory \"$env:LocalAppData\\Twinkstar\\User Data\"",
        "A-Install-App -Uninstaller \"$env:ProgramFiles\\Twinkstar Browser\\Uninstall.exe\" -ArgumentList '-silent'",
        "A-Hold-App"
    ],
    "post_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Uninstall-Manually \"$env:ProgramFiles\\Twinkstar Browser\"",
        "A-Remove-Link",
        "A-Remove-TempData \"$env:LocalAppData\\Twinkstar\""
    ],
    "post_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "$InstallerInfo, $out = A-Get-InstallerInfoFromWinget 'Twinkstar.TwinkstarBrowser' 'exe'",
            "$out"
        ],
        "regex": "(?:.*ver:(?<version>[^;]+))(?:.*x64:(?<x64>[^;]+))?(?:.*x86:(?<x86>[^;]+))?(?:.*arm64:(?<arm64>[^;]+))?"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "$matchX64",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.x64_machine.InstallerSha256"
                }
            }
        }
    }
}
