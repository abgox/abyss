            "hash": "33bdb6838f4def6b824b4a05957926d6e48ab116d1f1cd6a8ce0ceca96afbfc3"
        },
        "arm64": {
            "url": "https://github.com/twpayne/chezmoi/releases/download/v2.65.1/chezmoi_2.65.1_windows_arm64.zip",
