{
    "version": "0.18.3",
    "description": "一款适用于在现有工作流程基础上处理同步分支的 Git 客户端 | A Git client for simultaneous branches on top of your existing workflow.",
    "homepage": "https://gitbutler.com/",
    "license": {
        "identifier": "FSL-1.1-MIT",
        "url": "https://github.com/gitbutlerapp/gitbutler/blob/master/LICENSE.md"
    },
    "architecture": {
        "64bit": {
            "url": "https://releases.gitbutler.com/releases/release/0.18.3-2698/windows/x86_64/GitButler_0.18.3_x64_en-US.msi#/dl.msi_",
            "hash": "95fab83132cb21ac20a3f921bd4eab08ecbf524f035f330dbaca23fa94640bd8"
        }
    },
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install",
        "A-Require-Admin",
        "A-New-LinkDirectory \"$env:AppData\\com.gitbutler.app\",\"$env:AppData\\gitbutler\"",
        "A-Install-Exe msi"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Require-Admin",
        "A-Stop-Process \"$env:ProgramFiles\\GitButler\"",
        "A-Remove-Link",
        "$ProductCode = A-Get-ProductCode '^GitButler$'",
        "A-Uninstall-Exe $ProductCode",
        "A-Remove-TempData \"$env:LocalAppData\\com.gitbutler.app\""
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "$InstallerInfo = A-Get-InstallerInfoFromWinget 'GitButler.GitButler' 'msi'",
            "\"ver:$($InstallerInfo.PackageVersion);\"",
            "\"x64:$($InstallerInfo.x64_machine.InstallerUrl);\"",
            "\"x86:$($InstallerInfo.x86_machine.InstallerUrl);\"",
            "\"arm64:$($InstallerInfo.arm64_machine.InstallerUrl);\""
        ],
        "regex": "ver:(?<version>[^;]+)(?:; x64:(?<x64>[^;]*))?(?:; x86:(?<x86>[^;]*))?(?:; arm64:(?<arm64>[^;]*))?"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "$matchX64#/dl.msi_",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.x64_machine.InstallerSha256"
                }
            }
        }
    }
}
