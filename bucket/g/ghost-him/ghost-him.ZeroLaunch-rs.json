    "homepage": "https://zerolaunch.ghost-him.com/",
    "license": {
        "identifier": "GPL-3.0-only",
        "url": "https://github.com/ghost-him/ZeroLaunch-rs/blob/main/LICENSE.txt"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/ghost-him/ZeroLaunch-rs/releases/download/v0.6.6/zerolaunch-rs_0.6.6_x64-setup.exe",
            "hash": "01e99037f511cd8fe46db923e2a09222bf9d0f82d75db1db6036344b004fc716"
        },
        "arm64": {
            "url": "https://github.com/ghost-him/ZeroLaunch-rs/releases/download/v0.6.6/zerolaunch-rs_0.6.6_arm64-setup.exe",
            "hash": "5d9e33793d2f55901e2f7d37ccd6ef204cea8fb1f106c140106e373b4cba4801"
        }
    },
    "shortcuts": [
        [
            "app\\zerolaunch-rs.exe",
            "zerolaunch-rs"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install",
        "A-New-LinkDirectory \"$env:AppData\\ZeroLaunch-rs\"",
        "A-Install-Exe -Uninstaller 'app\\uninstall.exe'"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Stop-Process",
        "A-Remove-Link",
        "A-Uninstall-Exe",
        "A-Remove-TempData \"$env:LocalAppData\\com.zerolaunch-rs.app\""
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "A-Get-VersionFromGithubAPI"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/ghost-him/ZeroLaunch-rs/releases/download/v$version/zerolaunch-rs_$version_x64-setup.exe"
            },
            "arm64": {
                "url": "https://github.com/ghost-him/ZeroLaunch-rs/releases/download/v$version/zerolaunch-rs_$version_arm64-setup.exe"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
