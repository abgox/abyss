            "hash": "d321ebf351a77d41c4407ac64667031e6ba88c8a4c4fc300c8e4cfda97063f3c"
        },
        "arm64": {
            "url": "https://github.com/ghost-him/ZeroLaunch-rs/releases/download/v0.6.7/zerolaunch-rs_0.6.7_arm64-setup.exe",
            "hash": "92d9f2e200c4b9d93c5ef4120639520219a2a5cd81ca4cab7a758e859e2469ca"
        }
    },
    "shortcuts": [
        [
            "app\\zerolaunch-rs.exe",
            "zerolaunch-rs"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install",
        "A-New-LinkDirectory \"$env:AppData\\ZeroLaunch-rs\"",
        "A-Install-App -Uninstaller 'app\\uninstall.exe'"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Stop-Process",
        "A-Remove-Link",
        "A-Uninstall-App",
        "A-Remove-TempData \"$env:LocalAppData\\com.zerolaunch-rs.app\""
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "A-Get-VersionFromGithubAPI"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/ghost-him/ZeroLaunch-rs/releases/download/v$version/zerolaunch-rs_$version_x64-setup.exe"
            },
            "arm64": {
                "url": "https://github.com/ghost-him/ZeroLaunch-rs/releases/download/v$version/zerolaunch-rs_$version_arm64-setup.exe"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
