{
    "version": "140.0.7312.0",
    "description": "(Dev 版本) 谷歌浏览器 - 快速、安全且免费的网络浏览器，专为现代网络打造。(Dev version) Chrome Browser - Fast, secure, and free web browser, built for the modern web.",
    "homepage": "https://www.google.com/intl/zh-CN/chrome/",
    "license": {
        "identifier": "Freeware",
        "url": "https://www.google.com/intl/zh-CN/chrome/terms/"
    },
    "notes": [
        "The installation directory: $env:ProgramFiles\\Google\\Chrome Dev",
        "Scoop only manages data, installation, update, and uninstallation operations."
    ],
    "notes-cn": [
        "实际安装目录: $env:ProgramFiles\\Google\\Chrome Dev",
        "Scoop 只管理数据(通过 Link)，安装、更新、卸载操作"
    ],
    "architecture": {
        "64bit": {
            "url": "https://dl.google.com/release2/chrome/adc74t77f7tulgmrsevscbvdaj6a_140.0.7312.0/140.0.7312.0_chrome_installer_uncompressed.exe",
            "hash": "55540645f17e63178965c915858eb8c641cd8b15d1e28e66a0a6e065fdb2a214"
        },
        "arm64": {
            "url": "https://dl.google.com/release2/chrome/ac3fjfxjbm7cgztyllz4g72oat6q_140.0.7312.0/140.0.7312.0_chrome_installer_uncompressed.exe",
            "hash": "d155af2e5b370f11b63d1753004cadb459eec83133971f9c47a3d0079c93d9ab"
        }
    },
    "shortcuts": [
        [
            "$env:ProgramFiles\\Google\\Chrome Dev\\Application\\chrome.exe",
            "Google Chrome Dev"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PreInstall",
        "A-Require-Admin",
        "A-Install-Exe -Uninstaller \"$env:ProgramFiles\\Google\\Chrome Dev\\Application\\$version\\Installer\\setup.exe\" -ArgumentList @('--chrome-dev','--do-not-launch-chrome','--system-level')",
        "A-New-LinkDirectory @(\"$env:LocalAppData\\Google\\Chrome Dev\\User Data\")"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PostInstall"
    ],
    "pre_uninstall": [
        "$bucket = if ($install.bucket) { $install.bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PreUninstall",
        "A-Require-Admin",
        "A-Uninstall-Exe -ArgumentList @('--chrome-dev','--uninstall','--system-level','--multi-install','--force-uninstall')",
        "A-Remove-Link"
    ],
    "post_uninstall": [
        "$bucket = if ($install.bucket) { $install.bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PostUninstall"
    ],
    "checkver": {
        "script": [
            ". \"$pwd\\bin\\utils.ps1\"",
            "$InstallerInfo = A-Get-InstallerInfoFromWinget 'Google.Chrome.Dev.EXE' 'exe'",
            "\"ver:$($InstallerInfo.PackageVersion);\"",
            "\"x64:$($InstallerInfo.x64_machine.InstallerUrl);\"",
            "\"x86:$($InstallerInfo.x86_machine.InstallerUrl);\"",
            "\"arm64:$($InstallerInfo.arm64_machine.InstallerUrl);\""
        ],
        "regex": "ver:(?<version>[^;]+)(?:; x64:(?<x64>[^;]*))?(?:; x86:(?<x86>[^;]*))?(?:; arm64:(?<arm64>[^;]*))?"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "$matchX64",
                "hash": {
                    "url": "bin/scoop-auto-check-update-temp-data.jsonc",
                    "jsonpath": "$.x64_machine.InstallerSha256"
                }
            },
            "arm64": {
                "url": "$matchArm64",
                "hash": {
                    "url": "bin/scoop-auto-check-update-temp-data.jsonc",
                    "jsonpath": "$.arm64_machine.InstallerSha256"
                }
            }
        }
    }
}
