{
    "version": "8.9.0",
    "description": "神经网络、深度学习和机器学习模型的可视化工具 | Visualizer for neural network, deep learning and machine learning models",
    "homepage": "https://netron.app/",
    "license": {
        "identifier": "MIT"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/lutzroeder/netron/releases/download/v8.9.0/Netron-Setup-8.9.0.exe",
            "hash": "1da935f471a62cd94bef2db3b0a51e716e75d2e4f80a6f74ae064905752c4c98"
        }
    },
    "bin": "app\\Netron.exe",
    "shortcuts": [
        [
            "app\\Netron.exe",
            "Netron"
        ]
    ],
    "pre_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install",
        "A-New-LinkDirectory \"$env:AppData\\Netron\"",
        "A-Install-App -Uninstaller 'app\\Uninstall Netron.exe'"
    ],
    "post_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Stop-Process",
        "A-Remove-Link",
        "A-Uninstall-App",
        "A-Remove-TempData \"$env:LocalAppData\\netron-updater\""
    ],
    "post_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "A-Get-VersionFromGithubAPI"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/lutzroeder/netron/releases/download/v$version/Netron-Setup-$version.exe"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
