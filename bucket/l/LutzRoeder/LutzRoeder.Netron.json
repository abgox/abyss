{
    "version": "8.8.3",
    "description": "神经网络、深度学习和机器学习模型的可视化工具 | Visualizer for neural network, deep learning and machine learning models",
    "homepage": "https://netron.app/",
    "license": {
        "identifier": "MIT"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/lutzroeder/netron/releases/download/v8.8.3/Netron-Setup-8.8.3.exe",
            "hash": "7b06fcfe78bc636bd28c859816c2b85d1b5b8cbedd9da624bd5cd4b55c4553a8"
        }
    },
    "bin": "app\\Netron.exe",
    "shortcuts": [
        [
            "app\\Netron.exe",
            "Netron"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install",
        "A-New-LinkDirectory \"$env:AppData\\Netron\"",
        "A-Install-App -Uninstaller 'app\\Uninstall Netron.exe'"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Stop-Process",
        "A-Remove-Link",
        "A-Uninstall-App",
        "A-Remove-TempData \"$env:LocalAppData\\netron-updater\""
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "A-Get-VersionFromGithubAPI"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/lutzroeder/netron/releases/download/v$version/Netron-Setup-$version.exe"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
