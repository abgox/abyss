{
    "version": "2.13.1",
    "description": "LiveKit 的命令行界面 | Command line interface to LiveKit.",
    "homepage": "https://livekit.io/",
    "license": {
        "identifier": "Apache-2.0"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/livekit/livekit-cli/releases/download/v2.13.1/lk_2.13.1_windows_amd64.zip",
            "hash": "0f5d03e4b689d9c6932dd67c27c59884863d1d1bda2889bc81dfa04ed900440d"
        },
        "arm64": {
            "url": "https://github.com/livekit/livekit-cli/releases/download/v2.13.1/lk_2.13.1_windows_arm64.zip",
            "hash": "2b9b96c909cc94f0e2bf5b07f02e1a4c6addd786c4eabaebf5bd87372df20b41"
        }
    },
    "extract_to": "app",
    "bin": "app\\lk.exe",
    "pre_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install"
    ],
    "post_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Stop-Process"
    ],
    "post_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "A-Get-VersionFromGithubAPI"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/livekit/livekit-cli/releases/download/v$version/lk_$version_windows_amd64.zip"
            },
            "arm64": {
                "url": "https://github.com/livekit/livekit-cli/releases/download/v$version/lk_$version_windows_arm64.zip"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
