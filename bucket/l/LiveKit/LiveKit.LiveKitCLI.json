    "homepage": "https://livekit.io/",
    "license": {
        "identifier": "Apache-2.0",
        "url": "https://github.com/livekit/livekit-cli/blob/main/LICENSE"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/livekit/livekit-cli/releases/download/v2.12.9/lk_2.12.9_windows_amd64.zip",
            "hash": "9a1aa2874d4c8e5158e4905fa8e1e4f44ef0f9f685538a7a5048355b3d7f396d"
        },
        "arm64": {
            "url": "https://github.com/livekit/livekit-cli/releases/download/v2.12.9/lk_2.12.9_windows_arm64.zip",
            "hash": "023bd4be188a5a0517fee6f8b99f1e75af0c6f5a554eb2687b6c62417c79d81e"
        }
    },
    "extract_to": "app",
    "bin": "app\\lk.exe",
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Stop-Process"
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "A-Get-VersionFromGithubAPI"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/livekit/livekit-cli/releases/download/v$version/lk_$version_windows_amd64.zip"
            },
            "arm64": {
                "url": "https://github.com/livekit/livekit-cli/releases/download/v$version/lk_$version_windows_arm64.zip"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
