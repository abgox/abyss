{
    "version": "6.3.2",
    "description": "适用于桌面端的简单跨平台双因素 (2FA) 认证器应用程序 | Simple cross-platform two-factor (2FA) authenticator app for desktop.",
    "homepage": "https://authme.levminer.com/",
    "license": {
        "identifier": "GPL-3.0-only",
        "url": "https://github.com/Levminer/authme/blob/dev/LICENSE.md"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/Levminer/authme/releases/download/6.3.2/authme-6.3.2-windows-x64.msi#/dl.msi_",
            "hash": "cf461b3c90c2e550b21107ddcd602c236b26b36c3a5a57c370250d9661d8927c"
        }
    },
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install",
        "A-Require-Admin",
        "A-New-LinkDirectory \"$env:LocalAppData\\com.levminer.authme\\EBWebView\\Default\\Local Storage\"",
        "A-Install-Exe msi"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Require-Admin",
        "$ProductCode = A-Get-ProductCode '^Authme$'",
        "A-Uninstall-Exe $ProductCode",
        "A-Remove-Link",
        "A-Remove-TempData \"$env:LocalAppData\\com.levminer.authme\""
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "A-Get-VersionFromGithubAPI"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/Levminer/authme/releases/download/$version/authme-$version-windows-x64.msi#/dl.msi_"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
