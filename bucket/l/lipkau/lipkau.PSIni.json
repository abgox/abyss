        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install"
    ],
    "post_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Stop-Process"
    ],
