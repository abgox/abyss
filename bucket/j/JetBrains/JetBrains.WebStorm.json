{
    "version": "2025.3.2",
    "description": "JetBrains 的 IDE 产品，用于 Web 开发 (JavaScript，TypeScript) | A IDE by JetBrains for web development (JavaScript, TypeScript)",
    "homepage": "https://www.jetbrains.com/webstorm/",
    "license": {
        "identifier": "Unknown",
        "url": "https://www.jetbrains.com/store/license.html"
    },
    "architecture": {
        "64bit": {
            "url": "https://download.jetbrains.com/webstorm/WebStorm-2025.3.2.exe",
            "hash": "a4a839ac96394cf956c968c44efaa5107847f26e50937e6cf153a6321d646240"
        },
        "arm64": {
            "url": "https://download.jetbrains.com/webstorm/WebStorm-2025.3.2-aarch64.exe",
            "hash": "8d579539e52cb4a2b85beb95fe569cdc22b5f302f7ae856eca904495cc50d86f"
        }
    },
    "bin": [
        [
            "app\\bin\\webstorm64.exe",
            "WebStorm"
        ]
    ],
    "shortcuts": [
        [
            "app\\bin\\webstorm64.exe",
            "WebStorm 2025.3.2"
        ]
    ],
    "pre_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install",
        "A-Require-Admin",
        "$versionPart = $version -split '\\.'",
        "$versionShort = \"$($versionPart[0]).$($versionPart[1])\"",
        "$DataDir = \"WebStorm$versionShort\"",
        "A-New-LinkDirectory \"$env:AppData\\JetBrains\\$DataDir\"",
        "$LogLocation = '\"' + \"$env:TEMP\\scoop-$app-logs\" + '\"'",
        "A-Install-App -Uninstaller 'app\\bin\\Uninstall.exe' -ArgumentList '/S', \"/LOG=$LogLocation\", \"/D=$dir\\app\""
    ],
    "post_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Require-Admin",
        "A-Stop-Process",
        "A-Uninstall-App -ArgumentList '/S'",
        "A-Remove-Link",
        "$versionPart = $version -split '\\.'",
        "$versionShort = \"$($versionPart[0]).$($versionPart[1])\"",
        "$DataDir = \"WebStorm$versionShort\"",
        "A-Remove-TempData \"$env:LocalAppData\\JetBrains\\$DataDir\""
    ],
    "post_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "$InstallerInfo, $out = A-Get-InstallerInfoFromWinget 'JetBrains.WebStorm' 'exe'",
            "$out"
        ],
        "regex": "(?:.*ver:(?<version>[^;]+))(?:.*x64:(?<x64>[^;]+))?(?:.*x86:(?<x86>[^;]+))?(?:.*arm64:(?<arm64>[^;]+))?"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.jetbrains.com/webstorm/WebStorm-$version.exe",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.x64_user.InstallerSha256"
                }
            },
            "arm64": {
                "url": "https://download.jetbrains.com/webstorm/WebStorm-$version-aarch64.exe",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.arm64_user.InstallerSha256"
                }
            }
        },
        "shortcuts": [
            [
                "app\\bin\\webstorm64.exe",
                "WebStorm $version"
            ]
        ]
    }
}
