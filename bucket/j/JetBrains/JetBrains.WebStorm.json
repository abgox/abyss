{
    "version": "2025.3.3",
    "description": "JetBrains 的 IDE 产品，用于 Web 开发 (JavaScript，TypeScript) | A IDE by JetBrains for web development (JavaScript, TypeScript)",
    "homepage": "https://www.jetbrains.com/webstorm/",
    "license": {
        "identifier": "Unknown",
        "url": "https://www.jetbrains.com/store/license.html"
    },
    "architecture": {
        "64bit": {
            "url": "https://download.jetbrains.com/webstorm/WebStorm-2025.3.3.exe",
            "hash": "9426798b80f8ba960ee96cc846bbcc54b593ea62563fdb503eb509548204f10a"
        },
        "arm64": {
            "url": "https://download.jetbrains.com/webstorm/WebStorm-2025.3.3-aarch64.exe",
            "hash": "5163d2768cd290fc1cdfdc0aed75b0141290f75006aee46fbc31a9fbad7c9a72"
        }
    },
    "bin": [
        [
            "app\\bin\\webstorm64.exe",
            "WebStorm"
        ]
    ],
    "shortcuts": [
        [
            "app\\bin\\webstorm64.exe",
            "WebStorm 2025.3.3"
        ]
    ],
    "pre_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install",
        "A-Require-Admin",
        "$versionPart = $version -split '\\.'",
        "$versionShort = \"$($versionPart[0]).$($versionPart[1])\"",
        "$DataDir = \"WebStorm$versionShort\"",
        "A-New-LinkDirectory \"$env:AppData\\JetBrains\\$DataDir\"",
        "$LogLocation = '\"' + \"$env:TEMP\\scoop-$app-logs\" + '\"'",
        "A-Install-App -Uninstaller 'app\\bin\\Uninstall.exe' -ArgumentList '/S', \"/LOG=$LogLocation\", \"/D=$dir\\app\""
    ],
    "post_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Require-Admin",
        "A-Stop-Process",
        "A-Uninstall-App -ArgumentList '/S'",
        "A-Remove-Link",
        "$versionPart = $version -split '\\.'",
        "$versionShort = \"$($versionPart[0]).$($versionPart[1])\"",
        "$DataDir = \"WebStorm$versionShort\"",
        "A-Remove-TempData \"$env:LocalAppData\\JetBrains\\$DataDir\""
    ],
    "post_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "$res, $out = A-Get-InstallerInfoFromWinget 'JetBrains.WebStorm' 'exe'",
            "$out"
        ],
        "regex": "(?:.*ver:(?<version>[^;]+))(?:.*x64:(?<x64>[^;]+))?(?:.*x86:(?<x86>[^;]+))?(?:.*arm64:(?<arm64>[^;]+))?"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.jetbrains.com/webstorm/WebStorm-$version.exe",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.x64_user.InstallerSha256"
                }
            },
            "arm64": {
                "url": "https://download.jetbrains.com/webstorm/WebStorm-$version-aarch64.exe",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.arm64_user.InstallerSha256"
                }
            }
        },
        "shortcuts": [
            [
                "app\\bin\\webstorm64.exe",
                "WebStorm $version"
            ]
        ]
    }
}
