{
    "version": "2025.3",
    "description": "JetBrains 的 IDE 产品，用于 Web 开发 (JavaScript，TypeScript) | A IDE by JetBrains for web development (JavaScript, TypeScript).",
    "homepage": "https://www.jetbrains.com/webstorm/",
    "license": {
        "identifier": "Unknown",
        "url": "https://www.jetbrains.com/store/license.html"
    },
    "architecture": {
        "64bit": {
            "url": "https://download.jetbrains.com/webstorm/WebStorm-2025.3.exe",
            "hash": "d6f71e6d358722075402337ec93077dd04f8f1ccb55bd9ef3321342a9b67e33b"
        },
        "arm64": {
            "url": "https://download.jetbrains.com/webstorm/WebStorm-2025.3-aarch64.exe",
            "hash": "4da4c1017117d9dfa91a0df67cc852aa5fca3373de767578e2c6e10bff63cea1"
        }
    },
    "bin": [
        [
            "app\\bin\\webstorm64.exe",
            "WebStorm"
        ]
    ],
    "shortcuts": [
        [
            "app\\bin\\webstorm64.exe",
            "WebStorm 2025.3"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Install",
        "A-Require-Admin",
        "$versionPart = $version -split '\\.'",
        "$versionShort = \"$($versionPart[0]).$($versionPart[1])\"",
        "$DataDir = \"WebStorm$versionShort\"",
        "A-New-LinkDirectory \"$env:AppData\\JetBrains\\$DataDir\"",
        "$LogLocation = '\"' + \"$env:TEMP\\scoop-$app-logs\" + '\"'",
        "A-Install-Exe -Uninstaller 'app\\bin\\Uninstall.exe' -ArgumentList '/S', \"/LOG=$LogLocation\", \"/D=$dir\\app\""
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Uninstall",
        "A-Require-Admin",
        "A-Stop-Process",
        "A-Uninstall-Exe -ArgumentList '/S'",
        "A-Remove-Link",
        "$versionPart = $version -split '\\.'",
        "$versionShort = \"$($versionPart[0]).$($versionPart[1])\"",
        "$DataDir = \"WebStorm$versionShort\"",
        "A-Remove-TempData \"$env:LocalAppData\\JetBrains\\$DataDir\""
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". \"$pwd\\bin\\utils.ps1\"",
            "$InstallerInfo = A-Get-InstallerInfoFromWinget 'JetBrains.WebStorm' 'exe'",
            "\"ver:$($InstallerInfo.PackageVersion);\"",
            "\"x64:$($InstallerInfo.x64_user.InstallerUrl);\"",
            "\"x86:$($InstallerInfo.x86_user.InstallerUrl);\"",
            "\"arm64:$($InstallerInfo.arm64_user.InstallerUrl);\""
        ],
        "regex": "ver:(?<version>[^;]+)(?:; x64:(?<x64>[^;]*))?(?:; x86:(?<x86>[^;]*))?(?:; arm64:(?<arm64>[^;]*))?"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.jetbrains.com/webstorm/WebStorm-$version.exe",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.x64_user.InstallerSha256"
                }
            },
            "arm64": {
                "url": "https://download.jetbrains.com/webstorm/WebStorm-$version-aarch64.exe",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.arm64_user.InstallerSha256"
                }
            }
        },
        "shortcuts": [
            [
                "app\\bin\\webstorm64.exe",
                "WebStorm $version"
            ]
        ]
    }
}
