{
    "version": "2025.1.2.1",
    "description": "JetBrains 的 IDE 产品，用于数据分析、数据科学和机器学习。A IDE by JetBrains for data analysis, data science and machine learning.",
    "homepage": "https://www.jetbrains.com/dataspell/",
    "license": {
        "identifier": "Unknown",
        "url": "https://www.jetbrains.com/store/license.html"
    },
    "architecture": {
        "64bit": {
            "url": "https://download.jetbrains.com/python/dataspell-2025.1.2.1.exe",
            "hash": "c53b491ae1fa6353682371fb07f443f98ba25b5ebc21662477ae51839fb884fd"
        },
        "arm64": {
            "url": "https://download.jetbrains.com/python/dataspell-2025.1.2.1-aarch64.exe",
            "hash": "e2b00e795565ab6b1a6b1d0e3ed99874aa6212b9d4c1f9b78249978c2c994350"
        }
    },
    "bin": [
        [
            "App\\bin\\dataspell64.exe",
            "DataSpell"
        ]
    ],
    "shortcuts": [
        [
            "App\\bin\\dataspell64.exe",
            "DataSpell 2025.1.2.1"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PreInstall",
        "A-Require-Admin",
        "$LogLocation = '\"' + \"$env:TEMP\\scoop-$app-logs\" + '\"'",
        "A-Install-Exe -Uninstaller 'App\\bin\\Uninstall.exe' -ArgumentList @('/S', \"/LOG=$LogLocation\", \"/D=$dir\\App\") -Timeout 600",
        "$versionPart = $version -split '\\.'",
        "$versionShort = \"$($versionPart[0]).$($versionPart[1])\"",
        "$DataDir = \"DataSpell$versionShort\"",
        "A-New-LinkDirectory @(\"$env:AppData\\JetBrains\\$DataDir\")"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PostInstall"
    ],
    "pre_uninstall": [
        "$bucket = if ($install.bucket) { $install.bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PreUninstall",
        "A-Require-Admin",
        "A-Stop-Process",
        "A-Uninstall-Exe -ArgumentList @('/S')",
        "A-Remove-Link",
        "$versionPart = $version -split '\\.'",
        "$versionShort = \"$($versionPart[0]).$($versionPart[1])\"",
        "$DataDir = \"DataSpell$versionShort\"",
        "A-Remove-TempData @(\"$env:LocalAppData\\JetBrains\\$DataDir\")"
    ],
    "post_uninstall": [
        "$bucket = if ($install.bucket) { $install.bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PostUninstall"
    ],
    "checkver": {
        "script": [
            ". \"$pwd\\bin\\utils.ps1\"",
            "$InstallerInfo = A-Get-InstallerInfoFromWinget 'JetBrains.DataSpell' 'exe'",
            "$InstallerInfo.PackageVersion"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.jetbrains.com/python/dataspell-$version.exe",
                "hash": {
                    "url": "bin/scoop-auto-check-update-temp-data.jsonc",
                    "jsonpath": "$.x64_user.InstallerSha256"
                }
            },
            "arm64": {
                "url": "https://download.jetbrains.com/python/dataspell-$version-aarch64.exe",
                "hash": {
                    "url": "bin/scoop-auto-check-update-temp-data.jsonc",
                    "jsonpath": "$.arm64_user.InstallerSha256"
                }
            }
        },
        "shortcuts": [
            [
                "App\\bin\\dataspell64.exe",
                "DataSpell $version"
            ]
        ]
    }
}
