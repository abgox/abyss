{
    "version": "2025.2.6",
    "description": "(社区版本) JetBrains 的 IDE 产品，用于 Python 开发 | (Community Edition) A IDE by JetBrains for Python development.",
    "homepage": "https://www.jetbrains.com/pycharm/",
    "license": {
        "identifier": "Unknown",
        "url": "https://www.jetbrains.com/store/license.html"
    },
    "architecture": {
        "64bit": {
            "url": "https://download.jetbrains.com/python/pycharm-community-2025.2.6.exe",
            "hash": "f5a72fa2394da89ac54d12168e3893605eebd6f5c27afb0a5fc3e5c0f62b5970"
        },
        "arm64": {
            "url": "https://download.jetbrains.com/python/pycharm-community-2025.2.6-aarch64.exe",
            "hash": "745205ac9af69551b3306264a614a3ee9143c2cdbadc713302019e76b3742e3b"
        }
    },
    "bin": [
        [
            "app\\bin\\pycharm64.exe",
            "PyCharmCE"
        ]
    ],
    "shortcuts": [
        [
            "app\\bin\\pycharm64.exe",
            "PyCharm Community Edition 2025.2.6"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install",
        "A-Require-Admin",
        "$versionPart = $version -split '\\.'",
        "$versionShort = \"$($versionPart[0]).$($versionPart[1])\"",
        "$DataDir = \"PyCharmCE$versionShort\"",
        "A-New-LinkDirectory \"$env:AppData\\JetBrains\\$DataDir\"",
        "$LogLocation = '\"' + \"$env:TEMP\\scoop-$app-logs\" + '\"'",
        "A-Install-App -Uninstaller 'app\\bin\\Uninstall.exe' -ArgumentList '/S', \"/LOG=$LogLocation\", \"/D=$dir\\app\""
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Require-Admin",
        "A-Stop-Process",
        "A-Uninstall-App -ArgumentList '/S'",
        "A-Remove-Link",
        "$versionPart = $version -split '\\.'",
        "$versionShort = \"$($versionPart[0]).$($versionPart[1])\"",
        "$DataDir = \"PyCharmCE$versionShort\"",
        "A-Remove-TempData \"$env:LocalAppData\\JetBrains\\$DataDir\""
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "$InstallerInfo, $out = A-Get-InstallerInfoFromWinget 'JetBrains.PyCharm.Community' 'exe'",
            "$out"
        ],
        "regex": "(?:.*ver:(?<version>[^;]+))(?:.*x64:(?<x64>[^;]+))?(?:.*x86:(?<x86>[^;]+))?(?:.*arm64:(?<arm64>[^;]+))?"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.jetbrains.com/python/pycharm-community-$version.exe",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.x64_user.InstallerSha256"
                }
            },
            "arm64": {
                "url": "https://download.jetbrains.com/python/pycharm-community-$version-aarch64.exe",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.arm64_user.InstallerSha256"
                }
            }
        },
        "shortcuts": [
            [
                "app\\bin\\pycharm64.exe",
                "PyCharm Community Edition $version"
            ]
        ]
    }
}
