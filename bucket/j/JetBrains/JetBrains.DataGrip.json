{
    "version": "2025.3",
    "description": "JetBrains 的 IDE 产品，用于处理数据库 | A IDE by JetBrains for database processing.",
    "homepage": "https://www.jetbrains.com/datagrip/",
    "license": {
        "identifier": "Unknown",
        "url": "https://www.jetbrains.com/store/license.html"
    },
    "architecture": {
        "64bit": {
            "url": "https://download.jetbrains.com/datagrip/datagrip-2025.3.exe",
            "hash": "0f6f16c2f8b6d82844785c454a5b8f99bfea5840bfd45a9a760f195ad2bab452"
        },
        "arm64": {
            "url": "https://download.jetbrains.com/datagrip/datagrip-2025.3-aarch64.exe",
            "hash": "77192ba93fec70818f3532a67514b8847531cd74e336cf9cb2ed6b30bf25470c"
        }
    },
    "bin": [
        [
            "app\\bin\\datagrip64.exe",
            "DataGrip"
        ]
    ],
    "shortcuts": [
        [
            "app\\bin\\datagrip64.exe",
            "DataGrip 2025.3"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Install",
        "A-Require-Admin",
        "$versionPart = $version -split '\\.'",
        "$versionShort = \"$($versionPart[0]).$($versionPart[1])\"",
        "$DataDir = \"DataGrip$versionShort\"",
        "A-New-LinkDirectory @(\"$env:AppData\\JetBrains\\$DataDir\")",
        "$LogLocation = '\"' + \"$env:TEMP\\scoop-$app-logs\" + '\"'",
        "A-Install-Exe -Uninstaller 'app\\bin\\Uninstall.exe' -ArgumentList @('/S', \"/LOG=$LogLocation\", \"/D=$dir\\app\")"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Uninstall",
        "A-Require-Admin",
        "A-Stop-Process",
        "A-Uninstall-Exe -ArgumentList @('/S')",
        "A-Remove-Link",
        "$versionPart = $version -split '\\.'",
        "$versionShort = \"$($versionPart[0]).$($versionPart[1])\"",
        "$DataDir = \"DataGrip$versionShort\"",
        "A-Remove-TempData @(\"$env:LocalAppData\\JetBrains\\$DataDir\")"
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". \"$pwd\\bin\\utils.ps1\"",
            "$InstallerInfo = A-Get-InstallerInfoFromWinget 'JetBrains.DataGrip' 'exe'",
            "\"ver:$($InstallerInfo.PackageVersion);\"",
            "\"x64:$($InstallerInfo.x64_user.InstallerUrl);\"",
            "\"x86:$($InstallerInfo.x86_user.InstallerUrl);\"",
            "\"arm64:$($InstallerInfo.arm64_user.InstallerUrl);\""
        ],
        "regex": "ver:(?<version>[^;]+)(?:; x64:(?<x64>[^;]*))?(?:; x86:(?<x86>[^;]*))?(?:; arm64:(?<arm64>[^;]*))?"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.jetbrains.com/datagrip/datagrip-$version.exe",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.x64_user.InstallerSha256"
                }
            },
            "arm64": {
                "url": "https://download.jetbrains.com/datagrip/datagrip-$version-aarch64.exe",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.arm64_user.InstallerSha256"
                }
            }
        },
        "shortcuts": [
            [
                "app\\bin\\datagrip64.exe",
                "DataGrip $version"
            ]
        ]
    }
}
