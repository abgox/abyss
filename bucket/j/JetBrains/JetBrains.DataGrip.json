{
    "version": "2025.3.3",
    "description": "JetBrains 的 IDE 产品，用于处理数据库 | A IDE by JetBrains for database processing.",
    "homepage": "https://www.jetbrains.com/datagrip/",
    "license": {
        "identifier": "Unknown",
        "url": "https://www.jetbrains.com/store/license.html"
    },
    "architecture": {
        "64bit": {
            "url": "https://download.jetbrains.com/datagrip/datagrip-2025.3.3.exe",
            "hash": "bc30dadee53d4e5d8256b6a1ef4d4fe057f3956afb934acd6be9a8ac9f54dcae"
        },
        "arm64": {
            "url": "https://download.jetbrains.com/datagrip/datagrip-2025.3.3-aarch64.exe",
            "hash": "4a5ed9ad18300a7a4915d9e9a66e7e5726117a6891bc2f274bc1f70bf6eda245"
        }
    },
    "bin": [
        [
            "app\\bin\\datagrip64.exe",
            "DataGrip"
        ]
    ],
    "shortcuts": [
        [
            "app\\bin\\datagrip64.exe",
            "DataGrip 2025.3.3"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install",
        "A-Require-Admin",
        "$versionPart = $version -split '\\.'",
        "$versionShort = \"$($versionPart[0]).$($versionPart[1])\"",
        "$DataDir = \"DataGrip$versionShort\"",
        "A-New-LinkDirectory \"$env:AppData\\JetBrains\\$DataDir\"",
        "$LogLocation = '\"' + \"$env:TEMP\\scoop-$app-logs\" + '\"'",
        "A-Install-App -Uninstaller 'app\\bin\\Uninstall.exe' -ArgumentList '/S', \"/LOG=$LogLocation\", \"/D=$dir\\app\""
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Require-Admin",
        "A-Stop-Process",
        "A-Uninstall-App -ArgumentList '/S'",
        "A-Remove-Link",
        "$versionPart = $version -split '\\.'",
        "$versionShort = \"$($versionPart[0]).$($versionPart[1])\"",
        "$DataDir = \"DataGrip$versionShort\"",
        "A-Remove-TempData \"$env:LocalAppData\\JetBrains\\$DataDir\""
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "$InstallerInfo, $out = A-Get-InstallerInfoFromWinget 'JetBrains.DataGrip' 'exe'",
            "$out"
        ],
        "regex": "(?:.*ver:(?<version>[^;]+))(?:.*x64:(?<x64>[^;]+))?(?:.*x86:(?<x86>[^;]+))?(?:.*arm64:(?<arm64>[^;]+))?"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.jetbrains.com/datagrip/datagrip-$version.exe",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.x64_user.InstallerSha256"
                }
            },
            "arm64": {
                "url": "https://download.jetbrains.com/datagrip/datagrip-$version-aarch64.exe",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.arm64_user.InstallerSha256"
                }
            }
        },
        "shortcuts": [
            [
                "app\\bin\\datagrip64.exe",
                "DataGrip $version"
            ]
        ]
    }
}
