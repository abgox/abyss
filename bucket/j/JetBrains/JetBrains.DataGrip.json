{
    "version": "2025.2.5",
    "description": "JetBrains 的 IDE 产品，用于处理数据库 | A IDE by JetBrains for database processing.",
    "homepage": "https://www.jetbrains.com/datagrip/",
    "license": {
        "identifier": "Unknown",
        "url": "https://www.jetbrains.com/store/license.html"
    },
    "architecture": {
        "64bit": {
            "url": "https://download.jetbrains.com/datagrip/datagrip-2025.2.5.exe",
            "hash": "8a3167358da13ed9a4d21424acd2c74b5227aa401396e89529b1dbc6f9e9d85f"
        },
        "arm64": {
            "url": "https://download.jetbrains.com/datagrip/datagrip-2025.2.5-aarch64.exe",
            "hash": "87c7ba78e0fdd5885de861ad744816d3f719a9719e87ee3613d9ba2b4ffa19b2"
        }
    },
    "bin": [
        [
            "app\\bin\\datagrip64.exe",
            "DataGrip"
        ]
    ],
    "shortcuts": [
        [
            "app\\bin\\datagrip64.exe",
            "DataGrip 2025.2.5"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Install",
        "A-Require-Admin",
        "$versionPart = $version -split '\\.'",
        "$versionShort = \"$($versionPart[0]).$($versionPart[1])\"",
        "$DataDir = \"DataGrip$versionShort\"",
        "A-New-LinkDirectory @(\"$env:AppData\\JetBrains\\$DataDir\")",
        "$LogLocation = '\"' + \"$env:TEMP\\scoop-$app-logs\" + '\"'",
        "A-Install-Exe -Uninstaller 'app\\bin\\Uninstall.exe' -ArgumentList @('/S', \"/LOG=$LogLocation\", \"/D=$dir\\app\")"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Uninstall",
        "A-Require-Admin",
        "A-Stop-Process",
        "A-Uninstall-Exe -ArgumentList @('/S')",
        "A-Remove-Link",
        "$versionPart = $version -split '\\.'",
        "$versionShort = \"$($versionPart[0]).$($versionPart[1])\"",
        "$DataDir = \"DataGrip$versionShort\"",
        "A-Remove-TempData @(\"$env:LocalAppData\\JetBrains\\$DataDir\")"
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". \"$pwd\\bin\\utils.ps1\"",
            "$InstallerInfo = A-Get-InstallerInfoFromWinget 'JetBrains.DataGrip' 'exe'",
            "$InstallerInfo.PackageVersion"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.jetbrains.com/datagrip/datagrip-$version.exe",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.x64_user.InstallerSha256"
                }
            },
            "arm64": {
                "url": "https://download.jetbrains.com/datagrip/datagrip-$version-aarch64.exe",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.arm64_user.InstallerSha256"
                }
            }
        },
        "shortcuts": [
            [
                "app\\bin\\datagrip64.exe",
                "DataGrip $version"
            ]
        ]
    }
}
