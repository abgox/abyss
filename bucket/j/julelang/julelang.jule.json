{
    "version": "0.2.0",
    "description": "一种高效的编程语言，能够用于构建高效、快速、可靠且安全的软件，同时保持简洁性 | An effective programming language to build efficient, fast, reliable and safe software while maintaining simplicity",
    "homepage": "https://jule.dev/",
    "license": {
        "identifier": "BSD-3-Clause"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/julelang/jule/releases/download/jule0.2.0/jule-windows-amd64.zip",
            "hash": "3cf87a1548628a58369b368766e774ebc439e4f81f75589a378aca81e75c7a74"
        },
        "arm64": {
            "url": "https://github.com/julelang/jule/releases/download/jule0.2.0/jule-windows-arm64.zip",
            "hash": "5a0c327878dbb047a501f313271032e8faa5fa860ba0ecba6b053eb86197fa31"
        }
    },
    "extract_dir": "jule",
    "extract_to": "app",
    "env_add_path": "app\\bin",
    "commands": "julec",
    "pre_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install"
    ],
    "post_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Stop-Process"
    ],
    "post_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "A-Get-VersionFromGithubAPI"
        ],
        "regex": "jule(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/julelang/jule/releases/download/jule$version/jule-windows-amd64.zip"
            },
            "arm64": {
                "url": "https://github.com/julelang/jule/releases/download/jule$version/jule-windows-arm64.zip"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
