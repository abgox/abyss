{
    "version": "0.6.10",
    "description": "LLM API 管理 & 分发系统，通过标准的 OpenAI API 格式访问所有的大模型 | LLM API management & key redistribution system, providing access to all large models in standard OpenAI API format.",
    "homepage": "https://github.com/songquanpeng/one-api",
    "license": {
        "identifier": "MIT"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/songquanpeng/one-api/releases/download/v0.6.10/one-api.exe",
            "hash": "90bff24c61f84165ede220770b52d9cec2af3689b349054232a5c4ec582049ca"
        }
    },
    "bin": "one-api.exe",
    "pre_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install"
    ],
    "post_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Stop-Process"
    ],
    "post_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "A-Get-VersionFromGithubAPI"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/songquanpeng/one-api/releases/download/v$version/one-api.exe"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
