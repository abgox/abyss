    "license": {
        "identifier": "MIT",
        "url": "https://github.com/VSCodium/vscodium/blob/master/LICENSE"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/VSCodium/vscodium/releases/download/1.106.37943/VSCodiumUserSetup-x64-1.106.37943.exe",
            "hash": "5947403a64512b3a7f57e4d55f51a5cb444d596cddc52cf0d60df403042ca2d2"
        },
        "arm64": {
            "url": "https://github.com/VSCodium/vscodium/releases/download/1.106.37943/VSCodiumUserSetup-arm64-1.106.37943.exe",
            "hash": "6e18c9073f6e76f68b06bbe269a793d9a4cb60a460ba7eec8eb92b187926afa9"
        }
    },
    "env_add_path": "app\\bin",
    "shortcuts": [
        [
            "app\\VSCodium.exe",
            "VSCodium"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Install",
        "A-New-LinkDirectory @(\"$env:AppData\\VSCodium\",\"$env:UserProfile\\.vscode-oss\")",
        "A-Install-Exe inno"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Uninstall",
        "A-Stop-Process",
        "A-Remove-Link",
        "A-Uninstall-Exe"
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "url": "https://github.com/VSCodium/vscodium",
        "script": [
            ". \"$pwd\\bin\\utils.ps1\"",
            "A-Get-VersionFromGithubApi"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/VSCodium/vscodium/releases/download/$version/VSCodiumUserSetup-x64-$version.exe"
            },
            "arm64": {
                "url": "https://github.com/VSCodium/vscodium/releases/download/$version/VSCodiumUserSetup-arm64-$version.exe"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
