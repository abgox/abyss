        },
        "arm64": {
            "url": "https://cdn.powershellgallery.com/packages/pswritehtml.1.40.0.nupkg",
            "hash": "c768a8be5f3a1a8579b221d5837de535c4ae3cb64f9432c2658b6f9ca90c6245"
        }
    },
    "psmodule": {
        "name": "PSWriteHTML"
    },
    "pre_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install"
    ],
    "post_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Stop-Process"
    ],
    "post_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "url": "https://www.powershellgallery.com/packages/pswritehtml",
        "regex": "<h2>([\\d.]+)</h2>"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://cdn.powershellgallery.com/packages/pswritehtml.$version.nupkg"
            },
            "arm64": {
                "url": "https://cdn.powershellgallery.com/packages/pswritehtml.$version.nupkg"
            }
        }
    }
}
