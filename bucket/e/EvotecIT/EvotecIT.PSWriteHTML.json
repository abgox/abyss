    "homepage": "https://github.com/EvotecIT/PSWriteHTML",
    "license": {
        "identifier": "MIT"
    },
    "architecture": {
        "64bit": {
            "url": "https://cdn.powershellgallery.com/packages/pswritehtml.1.40.0.nupkg",
