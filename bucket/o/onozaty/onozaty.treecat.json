        },
        "arm64": {
            "url": "https://github.com/onozaty/treecat/releases/download/v1.1.1/treecat_v1.1.1_Windows_arm64.zip",
            "hash": "c3ef683d62e3b18e72a45a3e39b38271d0f3c31d95b1e0869db3b6b8b32b900f"
        }
    },
    "extract_to": "app",
    "bin": "app\\treecat.exe",
    "pre_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install"
    ],
    "post_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Stop-Process"
    ],
    "post_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "A-Get-VersionFromGithubAPI"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/onozaty/treecat/releases/download/v$version/treecat_v$version_Windows_x86_64.zip"
            },
            "arm64": {
                "url": "https://github.com/onozaty/treecat/releases/download/v$version/treecat_v$version_Windows_arm64.zip"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
