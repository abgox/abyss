{
    "version": "1.13.12",
    "description": "一个强大的 AI IDE | A powerful AI IDE",
    "homepage": "https://windsurf.com/",
    "license": {
        "identifier": "Unknown",
        "url": "https://windsurf.com/terms-of-service-individual"
    },
    "architecture": {
        "64bit": {
            "url": "https://windsurf-stable.codeiumdata.com/win32-x64-user/stable/aa644d109002a89ca18e1ba5fe9355859b4f0565/WindsurfUserSetup-x64-1.13.12.exe",
            "hash": "7032fc1304467cdb1233dd7072e8f0ee8730ef70b22ebed0a7a0890597e79bb5"
        }
    },
    "env_add_path": "app\\bin",
    "commands": "windsurf",
    "shortcuts": [
        [
            "app\\Windsurf.exe",
            "Windsurf"
        ]
    ],
    "pre_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install",
        "A-New-LinkDirectory \"$env:AppData\\Windsurf\",\"$home\\.windsurf\"",
        "A-Install-Inno"
    ],
    "post_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Stop-Process",
        "A-Remove-Link",
        "A-Uninstall-Inno"
    ],
    "post_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "$InstallerInfo, $out = A-Get-InstallerInfoFromWinget 'Codeium.Windsurf' 'exe'",
            "$out"
        ],
        "regex": "(?:.*ver:(?<version>[^;]+))(?:.*x64:(?<x64>[^;]+))?(?:.*x86:(?<x86>[^;]+))?(?:.*arm64:(?<arm64>[^;]+))?"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "$matchX64",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.x64_user.InstallerSha256"
                }
            }
        }
    }
}
