{
    "version": "1.7.15",
    "description": "一款支持多个大语言模型 (LLM) 供应商的桌面客户端 | A desktop client that supports for multiple LLM providers",
    "homepage": "https://cherry-ai.com/",
    "license": {
        "identifier": "Unknown"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/CherryHQ/cherry-studio/releases/download/v1.7.15/Cherry-Studio-1.7.15-x64-setup.exe",
            "hash": "dab32fca262aa4dc49d1b18a6ce3cec8b886d8aa9e9cd5fbe1ada01c4ec9d5ad"
        },
        "arm64": {
            "url": "https://github.com/CherryHQ/cherry-studio/releases/download/v1.7.15/Cherry-Studio-1.7.15-arm64-setup.exe",
            "hash": "d186b1a65ecf5e6b14c84d0ec7f997572e5588ba341c2883385e21a0d4df35f1"
        }
    },
    "shortcuts": [
        [
            "app\\Cherry Studio.exe",
            "Cherry Studio"
        ]
    ],
    "pre_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install",
        "A-New-LinkDirectory \"$env:AppData\\CherryStudio\"",
        "A-Install-App -Uninstaller 'app\\Uninstall Cherry Studio.exe'"
    ],
    "post_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Stop-Process",
        "A-Remove-Link",
        "A-Uninstall-App",
        "A-Remove-TempData \"$env:LocalAppData\\cherrystudio-updater\""
    ],
    "post_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "A-Get-VersionFromGithubAPI"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/CherryHQ/cherry-studio/releases/download/v$version/Cherry-Studio-$version-x64-setup.exe"
            },
            "arm64": {
                "url": "https://github.com/CherryHQ/cherry-studio/releases/download/v$version/Cherry-Studio-$version-arm64-setup.exe"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
