{
    "version": "1.7.12",
    "description": "一款支持多个大语言模型 (LLM) 供应商的桌面客户端 | A desktop client that supports for multiple LLM providers.",
    "homepage": "https://cherry-ai.com/",
    "license": {
        "identifier": "Unknown"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/CherryHQ/cherry-studio/releases/download/v1.7.12/Cherry-Studio-1.7.12-x64-setup.exe",
            "hash": "cb253588d8e1662c6386219524387005911a639a6bba61b2a9a675becf8746b4"
        },
        "arm64": {
            "url": "https://github.com/CherryHQ/cherry-studio/releases/download/v1.7.12/Cherry-Studio-1.7.12-arm64-setup.exe",
            "hash": "aa9599ee1a4bf10e26ecb8311a05c416271daf810f942c09d082a9fa030be6c2"
        }
    },
    "shortcuts": [
        [
            "app\\Cherry Studio.exe",
            "Cherry Studio"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install",
        "A-New-LinkDirectory \"$env:AppData\\CherryStudio\"",
        "A-Install-App -Uninstaller 'app\\Uninstall Cherry Studio.exe'"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Stop-Process",
        "A-Remove-Link",
        "A-Uninstall-App",
        "A-Remove-TempData \"$env:LocalAppData\\cherrystudio-updater\""
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "A-Get-VersionFromGithubAPI"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/CherryHQ/cherry-studio/releases/download/v$version/Cherry-Studio-$version-x64-setup.exe"
            },
            "arm64": {
                "url": "https://github.com/CherryHQ/cherry-studio/releases/download/v$version/Cherry-Studio-$version-arm64-setup.exe"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
