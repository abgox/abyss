{
    "version": "0.8.8",
    "description": "ComfyUI 的桌面应用程序。ComfyUI 是一款强大的模块化扩散模型 GUI、API 和后台，具有图形/节点界面 | A desktop app for ComfyUI, the most powerful and modular diffusion model GUI, api and backend with a graph/nodes interface",
    "homepage": "https://comfy.org/",
    "license": {
        "identifier": "GPL-3.0-only"
    },
    "architecture": {
        "64bit": {
            "url": "https://download.todesktop.com/241012ess7yxs0e/ComfyUI%20Setup%200.8.8%20-%20Build%20260225bwr41lu87-x64.exe",
            "hash": "26307d1dfe3f5f5f8f7d7200963a956b116b2214d3e9da346727cd299bbd8e6b"
        }
    },
    "shortcuts": [
        [
            "app\\ComfyUI.exe",
            "ComfyUI"
        ]
    ],
    "pre_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install",
        "A-New-LinkDirectory \"$env:AppData\\ComfyUI\"",
        "A-Install-App -Uninstaller 'app\\Uninstall ComfyUI.exe'"
    ],
    "post_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Stop-Process",
        "A-Remove-Link",
        "A-Uninstall-App"
    ],
    "post_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "$res, $out = A-Get-InstallerInfoFromWinget 'Comfy.ComfyUI-Desktop' 'exe'",
            "$out"
        ],
        "regex": "(?:.*ver:(?<version>[^;]+))(?:.*x64:(?<x64>[^;]+))?(?:.*x86:(?<x86>[^;]+))?(?:.*arm64:(?<arm64>[^;]+))?"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "$matchX64",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.x64_user.InstallerSha256"
                }
            }
        }
    }
}
