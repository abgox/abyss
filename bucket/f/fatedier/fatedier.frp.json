{
    "version": "0.67.0",
    "description": "一款快速的反向代理，它能让位于网络地址转换 (NAT) 或防火墙后的本地服务器暴露在互联网上 | A fast reverse proxy to help you expose a local server behind a NAT or firewall to the internet",
    "homepage": "https://gofrp.org/",
    "license": {
        "identifier": "Apache-2.0"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/fatedier/frp/releases/download/v0.67.0/frp_0.67.0_windows_amd64.zip",
            "hash": "8baf23e3fbd486f6ba0913501372c5ff0053efa88a8b8d391f3605ced43d2af5",
            "extract_dir": "frp_0.67.0_windows_amd64"
        },
        "arm64": {
            "url": "https://github.com/fatedier/frp/releases/download/v0.67.0/frp_0.67.0_windows_arm64.zip",
            "hash": "3b58c0506d3767beec81b7a533b83b7a86147f118850fe127b0d53fa6e9bd244",
            "extract_dir": "frp_0.67.0_windows_arm64"
        }
    },
    "extract_to": "app",
    "bin": [
        [
            "app\\frpc.exe",
            "frpc",
            "-c \"$dir\\app\\frpc.toml\""
        ],
        [
            "app\\frps.exe",
            "frps",
            "-c \"$dir\\app\\frps.toml\""
        ]
    ],
    "persist": [
        [
            "app\\frpc.toml",
            "frpc.toml"
        ],
        [
            "app\\frps.toml",
            "frps.toml"
        ]
    ],
    "pre_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install"
    ],
    "post_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Stop-Process"
    ],
    "post_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "A-Get-VersionFromGithubAPI"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/fatedier/frp/releases/download/v$version/frp_$version_windows_amd64.zip",
                "extract_dir": "frp_$version_windows_amd64"
            },
            "arm64": {
                "url": "https://github.com/fatedier/frp/releases/download/v$version/frp_$version_windows_arm64.zip",
                "extract_dir": "frp_$version_windows_arm64"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
