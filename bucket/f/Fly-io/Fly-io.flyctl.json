{
    "version": "0.3.221",
    "description": "为 fly.io 服务提供的命令行工具 | Command line tools for fly.io services.",
    "homepage": "https://fly.io/",
    "license": {
        "identifier": "Apache-2.0",
        "url": "https://github.com/superfly/flyctl/blob/master/LICENSE"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/superfly/flyctl/releases/download/v0.3.221/flyctl_0.3.221_Windows_x86_64.zip",
            "hash": "e838410a05af49f7736cc44a74a1517a2ee04d980a4625f628263d92032abe3d"
        },
        "arm64": {
            "url": "https://github.com/superfly/flyctl/releases/download/v0.3.221/flyctl_0.3.221_Windows_arm64.zip",
            "hash": "0e77f58362a281616111658189c4efe06b4b7e14cc01b2677be26e3dd410d3d3"
        }
    },
    "extract_to": "app",
    "bin": [
        "app\\flyctl.exe",
        [
            "app\\flyctl.exe",
            "fly"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Install"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Uninstall",
        "A-Stop-Process"
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "url": "https://github.com/superfly/flyctl",
        "script": [
            ". \"$pwd\\bin\\utils.ps1\"",
            "A-Get-VersionFromGithubApi"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/superfly/flyctl/releases/download/v$version/flyctl_$version_Windows_x86_64.zip"
            },
            "arm64": {
                "url": "https://github.com/superfly/flyctl/releases/download/v$version/flyctl_$version_Windows_arm64.zip"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
