{
    "version": "0.4.14",
    "description": "为 fly.io 服务提供的命令行工具 | Command line tools for fly.io services",
    "homepage": "https://fly.io/",
    "license": {
        "identifier": "Apache-2.0"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/superfly/flyctl/releases/download/v0.4.14/flyctl_0.4.14_Windows_x86_64.zip",
            "hash": "fefccc01d8e1af5dc2284adad9845d924545b3b72c8e5154c11347793955222f"
        },
        "arm64": {
            "url": "https://github.com/superfly/flyctl/releases/download/v0.4.14/flyctl_0.4.14_Windows_arm64.zip",
            "hash": "ef55bc6ce174d8afa38b4b2f496577b2f3df32d2ed58e1e191cd79dc14d010dc"
        }
    },
    "extract_to": "app",
    "bin": [
        "app\\flyctl.exe",
        [
            "app\\flyctl.exe",
            "fly"
        ]
    ],
    "pre_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install"
    ],
    "post_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Stop-Process"
    ],
    "post_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "A-Get-VersionFromGithubAPI"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/superfly/flyctl/releases/download/v$version/flyctl_$version_Windows_x86_64.zip"
            },
            "arm64": {
                "url": "https://github.com/superfly/flyctl/releases/download/v$version/flyctl_$version_Windows_arm64.zip"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
