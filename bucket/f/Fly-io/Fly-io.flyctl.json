{
    "version": "0.3.223",
    "description": "为 fly.io 服务提供的命令行工具 | Command line tools for fly.io services.",
    "homepage": "https://fly.io/",
    "license": {
        "identifier": "Apache-2.0",
        "url": "https://github.com/superfly/flyctl/blob/master/LICENSE"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/superfly/flyctl/releases/download/v0.3.223/flyctl_0.3.223_Windows_x86_64.zip",
            "hash": "c19767b76eb6bdd7666b3ab5892ec3fce293ff41d2f52652af0921fad2bb8673"
        },
        "arm64": {
            "url": "https://github.com/superfly/flyctl/releases/download/v0.3.223/flyctl_0.3.223_Windows_arm64.zip",
            "hash": "fd2e042e8b0c9e61a4ac5767e6d4fe540165185fa425a393285f1bdc2fb2f552"
        }
    },
    "extract_to": "app",
    "bin": [
        "app\\flyctl.exe",
        [
            "app\\flyctl.exe",
            "fly"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Install"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Uninstall",
        "A-Stop-Process"
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "url": "https://github.com/superfly/flyctl",
        "script": [
            ". \"$pwd\\bin\\utils.ps1\"",
            "A-Get-VersionFromGithubApi"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/superfly/flyctl/releases/download/v$version/flyctl_$version_Windows_x86_64.zip"
            },
            "arm64": {
                "url": "https://github.com/superfly/flyctl/releases/download/v$version/flyctl_$version_Windows_arm64.zip"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
