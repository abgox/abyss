{
    "version": "5.4.11",
    "description": "一款跨平台的 SQL 编辑器和数据库管理器 | A cross-platform SQL editor and database manager.",
    "homepage": "https://www.beekeeperstudio.io/",
    "license": {
        "identifier": "Freemium",
        "url": "https://github.com/beekeeper-studio/beekeeper-studio/blob/master/LICENSE.md"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/beekeeper-studio/beekeeper-studio/releases/download/v5.4.11/Beekeeper-Studio-Setup-5.4.11.exe",
            "hash": "837cbf06d39c83cd4bf8d8413cd0c500b4a35e33c6fe1b8d84d65eff9ec5f55a"
        }
    },
    "shortcuts": [
        [
            "app\\Beekeeper Studio.exe",
            "Beekeeper Studio"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Install",
        "A-New-LinkDirectory @(\"$env:AppData\\beekeeper-studio\")",
        "A-Install-Exe -Uninstaller 'app\\Uninstall Beekeeper Studio.exe'"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Uninstall",
        "A-Stop-Process",
        "A-Remove-Link",
        "A-Uninstall-Exe",
        "A-Remove-TempData @(\"$env:LocalAppData\\beekeeper-studio-updater\")"
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "url": "https://github.com/beekeeper-studio/beekeeper-studio",
        "script": [
            ". \"$pwd\\bin\\utils.ps1\"",
            "A-Get-VersionFromGithubApi"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/beekeeper-studio/beekeeper-studio/releases/download/v$version/Beekeeper-Studio-Setup-$version.exe"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
