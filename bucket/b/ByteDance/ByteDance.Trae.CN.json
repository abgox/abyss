{
    "version": "1.0.27573",
    "description": "字节跳动的一个 AI 驱动的代码 IDE | An AI-powered code IDE by ByteDance.",
    "homepage": "https://www.trae.com.cn/",
    "license": {
        "identifier": "Unknown",
        "url": "https://www.trae.com.cn/terms-of-service"
    },
    "architecture": {
        "64bit": {
            "url": "https://lf-cdn.trae.com.cn/obj/trae-com-cn/pkg/app/releases/stable/1.0.27573/win32/Trae%20CN-Setup-x64.exe",
            "hash": "ade19419720f8568daacd07968f8692f476cef4e7a9355a7f0e00f35130a4664"
        }
    },
    "env_add_path": "app\\bin",
    "shortcuts": [
        [
            "app\\Trae CN.exe",
            "Trae CN"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install",
        "A-Require-Admin",
        "A-New-LinkDirectory \"$env:AppData\\Trae CN\",\"$home\\.trae-cn\"",
        "A-Install-Inno"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Require-Admin",
        "A-Stop-Process",
        "A-Remove-Link",
        "A-Uninstall-Inno"
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "url": "https://api.trae.com.cn/icube/api/v1/native/version/trae/cn/latest",
        "jsonpath": "$.data.manifest.win32.version"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://lf-cdn.trae.com.cn/obj/trae-com-cn/pkg/app/releases/stable/$version/win32/Trae%20CN-Setup-x64.exe"
            }
        }
    }
}
