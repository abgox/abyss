    },
    "architecture": {
        "64bit": {
            "url": "https://lf-cdn.trae.ai/obj/trae-ai-sg/pkg/app/releases/stable/2.3.9373/win32/Trae-Setup-x64.exe",
            "hash": "e5f01caba9e324537a9b61d9ed7ca3623ffd5561dc6963c667d034772ae78005"
        }
    },
    "env_add_path": "app\\bin",
    "commands": "trae",
    "shortcuts": [
        [
            "app\\Trae.exe",
            "Trae"
        ]
    ],
    "pre_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install",
        "A-Require-Admin",
        "A-New-LinkDirectory \"$env:AppData\\Trae\",\"$home\\.trae\"",
        "A-Install-Inno"
    ],
    "post_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Require-Admin",
        "A-Stop-Process",
        "A-Remove-Link",
        "A-Uninstall-Inno"
    ],
    "post_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "url": "https://api.trae.ai/icube/api/v1/native/version/trae/latest",
        "jsonpath": "$.data.manifest.win32.version"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://lf-cdn.trae.ai/obj/trae-ai-sg/pkg/app/releases/stable/$version/win32/Trae-Setup-x64.exe"
            }
        }
    }
}
