{
    "version": "0.2.3",
    "description": "一款带有交互式 TUI 的、快速的终端 Excel 查看器。 | A fast terminal Excel viewer with an interactive TUI.",
    "homepage": "https://github.com/bgreenwell/xleak",
    "license": {
        "identifier": "MIT",
        "url": "https://github.com/bgreenwell/xleak/blob/main/LICENSE"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/bgreenwell/xleak/releases/download/v0.2.3/xleak-x86_64-pc-windows-msvc.zip",
            "hash": "9d48c043cfc909b5ff0187c4d3ccb3143337849dacf33337e5a9027ad4ee1cc1"
        }
    },
    "extract_to": "app",
    "bin": "app\\xleak.exe",
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Install",
        "A-New-LinkDirectory @(\"$env:AppData\\xleak\")"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Uninstall",
        "A-Stop-Process",
        "A-Remove-Link"
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "url": "https://github.com/bgreenwell/xleak",
        "script": [
            ". \"$pwd\\bin\\utils.ps1\"",
            "A-Get-VersionFromGithubApi"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/bgreenwell/xleak/releases/download/v$version/xleak-x86_64-pc-windows-msvc.zip"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
