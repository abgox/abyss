{
    "version": "0.214.6",
    "description": "一款高性能的多人协作代码编辑器，由 Atom 和 Tree-sitter 的开发者打造 | A high-performance, multiplayer code editor from the creators of Atom and Tree-sitter.",
    "homepage": "https://zed.dev/",
    "license": {
        "identifier": "GPL-3.0-only",
        "url": "https://github.com/zed-industries/zed/blob/main/LICENSE-GPL"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/zed-industries/zed/releases/download/v0.214.6/Zed-x86_64.exe",
            "hash": "2a8208c84c7c7513fe7dae2c098cb0f6a3f19d6b94fa617781cae3a2aa1cea96"
        }
    },
    "env_add_path": "app\\bin",
    "shortcuts": [
        [
            "app\\Zed.exe",
            "Zed"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Install",
        "A-New-LinkDirectory @(\"$env:AppData\\Zed\",\"$env:LocalAppData\\Zed\")",
        "A-Install-Exe inno"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Uninstall",
        "A-Stop-Process",
        "A-Remove-Link",
        "A-Uninstall-Exe"
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "url": "https://github.com/zed-industries/zed",
        "script": [
            ". \"$pwd\\bin\\utils.ps1\"",
            "A-Get-VersionFromGithubApi"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/zed-industries/zed/releases/download/v$version/Zed-x86_64.exe"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
