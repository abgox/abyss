{
    "version": "1.14.4b",
    "description": "一款基于火狐的浏览器，旨在将您的工作效率提升到新高度。A firefox-based browser with the aim of pushing your productivity to a new level.",
    "homepage": "https://zen-browser.app/",
    "license": {
        "identifier": "MPL-2.0",
        "url": "https://github.com/zen-browser/desktop/blob/HEAD/LICENSE"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/zen-browser/desktop/releases/download/1.14.4b/zen.installer.exe",
            "hash": "e8cfea7d2329916bd262fc8da3adc02fde33d99b3f21930267de471e7f08023d"
        },
        "arm64": {
            "url": "https://github.com/zen-browser/desktop/releases/download/1.14.4b/zen.installer-arm64.exe",
            "hash": "fb42bea47c2af7a6cbe5933ea287a71a50e338946ec90fed6062fdbbb3ec8b2e"
        }
    },
    "shortcuts": [
        [
            "zen.exe",
            "Zen Browser"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PreInstall",
        "A-Require-Admin",
        "A-Install-Exe -Uninstaller 'uninstall\\helper.exe'",
        "A-New-LinkDirectory @(\"$env:AppData\\zen\")"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PostInstall"
    ],
    "pre_uninstall": [
        "$bucket = if ($install.bucket) { $install.bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PreUninstall",
        "A-Require-Admin",
        "A-Stop-Process",
        "A-Remove-Link",
        "A-Uninstall-Exe"
    ],
    "post_uninstall": [
        "$bucket = if ($install.bucket) { $install.bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PostUninstall"
    ],
    "checkver": {
        "github": "https://github.com/zen-browser/desktop",
        "script": [
            ". \"$pwd\\bin\\utils.ps1\"",
            "A-Get-Version"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/zen-browser/desktop/releases/download/$version/zen.installer.exe"
            },
            "arm64": {
                "url": "https://github.com/zen-browser/desktop/releases/download/$version/zen.installer-arm64.exe"
            }
        }
    }
}
