{
    "version": "1.13.1",
    "description": "一个 PowerToys Run 插件，用于搜索 GitHub 存储库，然后在默认浏览器中打开 | A PowerToys Run plugin for searching and opening GitHub repositories.",
    "homepage": "https://github.com/8LWXpg/PowerToysRun-GitHubRepo",
    "license": {
        "identifier": "MIT",
        "url": "https://github.com/8LWXpg/PowerToysRun-GitHubRepo/blob/master/LICENSE"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/8LWXpg/PowerToysRun-GitHubRepo/releases/download/v1.13.1/GitHubRepo-v1.13.1-x64.zip",
            "hash": "c3434686076f7aee97d36cc62ec6035a445e2ada1299d6caf0ed84b01965fe4f"
        },
        "arm64": {
            "url": "https://github.com/8LWXpg/PowerToysRun-GitHubRepo/releases/download/v1.13.1/GitHubRepo-v1.13.1-arm64.zip",
            "hash": "d7a3cbdb1f6befd843fe7bdba7edbb3477e35c69f39fa7b45013be027a21c627"
        }
    },
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Install",
        "A-Add-PowerToysRunPlugin 'GitHubRepo'"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Uninstall",
        "A-Remove-PowerToysRunPlugin"
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "url": "https://github.com/8LWXpg/PowerToysRun-GitHubRepo",
        "script": [
            ". \"$pwd\\bin\\utils.ps1\"",
            "A-Get-VersionFromGithubApi"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/8LWXpg/PowerToysRun-GitHubRepo/releases/download/v$version/GitHubRepo-v$version-x64.zip"
            },
            "arm64": {
                "url": "https://github.com/8LWXpg/PowerToysRun-GitHubRepo/releases/download/v$version/GitHubRepo-v$version-arm64.zip"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
