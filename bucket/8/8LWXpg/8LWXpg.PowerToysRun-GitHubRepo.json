{
    "version": "1.13.2",
    "description": "一个 PowerToys Run 插件，用于搜索 GitHub 存储库，然后在默认浏览器中打开 | A PowerToys Run plugin for searching and opening GitHub repositories.",
    "homepage": "https://github.com/8LWXpg/PowerToysRun-GitHubRepo",
    "license": {
        "identifier": "MIT"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/8LWXpg/PowerToysRun-GitHubRepo/releases/download/v1.13.2/GitHubRepo-v1.13.2-x64.zip",
            "hash": "5782ba4e93f80f07e25e5322f49902be04990ece37cc96889ac4e48fe734c5ee"
        },
        "arm64": {
            "url": "https://github.com/8LWXpg/PowerToysRun-GitHubRepo/releases/download/v1.13.2/GitHubRepo-v1.13.2-arm64.zip",
            "hash": "0e5157c55c526353d099b1f0bdcded55e894311165bdb067c7ec83fad1f0ee84"
        }
    },
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install",
        "A-Add-PowerToysRunPlugin 'GitHubRepo'"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall"
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "A-Get-VersionFromGithubAPI"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/8LWXpg/PowerToysRun-GitHubRepo/releases/download/v$version/GitHubRepo-v$version-x64.zip"
            },
            "arm64": {
                "url": "https://github.com/8LWXpg/PowerToysRun-GitHubRepo/releases/download/v$version/GitHubRepo-v$version-arm64.zip"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
