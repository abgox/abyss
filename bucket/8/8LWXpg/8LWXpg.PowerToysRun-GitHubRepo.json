{
    "version": "1.13.3",
    "description": "一个 PowerToys Run 插件，用于搜索 GitHub 存储库，然后在默认浏览器中打开 | A PowerToys Run plugin for searching and opening GitHub repositories",
    "homepage": "https://github.com/8LWXpg/PowerToysRun-GitHubRepo",
    "license": {
        "identifier": "MIT"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/8LWXpg/PowerToysRun-GitHubRepo/releases/download/v1.13.3/GitHubRepo-v1.13.3-x64.zip",
            "hash": "ddab62c6e56f9bfe8ecf8face640c8efc85686204fb39f303916fbbb18c9788e"
        },
        "arm64": {
            "url": "https://github.com/8LWXpg/PowerToysRun-GitHubRepo/releases/download/v1.13.3/GitHubRepo-v1.13.3-arm64.zip",
            "hash": "44d1cd547d95acf464a2df8d1cc426ef3697d02446468530cdfe10dfd18c0868"
        }
    },
    "pre_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install",
        "A-Add-PowerToysRunPlugin 'GitHubRepo'"
    ],
    "post_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall"
    ],
    "post_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "A-Get-VersionFromGithubAPI"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/8LWXpg/PowerToysRun-GitHubRepo/releases/download/v$version/GitHubRepo-v$version-x64.zip"
            },
            "arm64": {
                "url": "https://github.com/8LWXpg/PowerToysRun-GitHubRepo/releases/download/v$version/GitHubRepo-v$version-arm64.zip"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
