{
    "version": "7.0.32",
    "description": "一个免费，易于使用的工具，可帮助您收集，组织，注释，引用和共享研究 | A free, easy-to-use tool to help you collect, organize, annotate, cite, and share research.",
    "homepage": "https://www.zotero.org/",
    "license": {
        "identifier": "Freeware"
    },
    "architecture": {
        "64bit": {
            "url": "https://download.zotero.org/client/release/7.0.32/Zotero-7.0.32_x64_setup.exe#/dl.7z",
            "hash": "21fa7f3b632089c8fc7d391f36f5b3c2b063c9bf1cf2315200f4d79a955b1646"
        },
        "arm64": {
            "url": "https://download.zotero.org/client/release/7.0.32/Zotero-7.0.32_arm64_setup.exe#/dl.7z",
            "hash": "52b357ad3ffdae3b2d771defb01f36917e5d6a9e1e79e1400216f8aa54213b32"
        }
    },
    "extract_dir": "core",
    "extract_to": "app",
    "bin": "app\\zotero.exe",
    "shortcuts": [
        [
            "app\\zotero.exe",
            "Zotero"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install",
        "A-New-LinkDirectory \"$env:AppData\\Zotero\",\"$home\\Zotero\""
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Stop-Process",
        "A-Remove-Link",
        "A-Remove-TempData \"$env:ProgramData\\Zotero\""
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "url": "https://www.zotero.org/support/changelog",
        "regex": "Changes in ([\\d.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.zotero.org/client/release/$version/Zotero-$version_x64_setup.exe#/dl.7z"
            },
            "arm64": {
                "url": "https://download.zotero.org/client/release/$version/Zotero-$version_arm64_setup.exe#/dl.7z"
            }
        }
    }
}
