{
    "version": "1.10.17",
    "description": "一站式 OpenAI API 聚合服务 | All in Done Hub service for OpenAI API.",
    "homepage": "https://github.com/deanxv/done-hub",
    "license": {
        "identifier": "Apache-2.0"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/deanxv/done-hub/releases/download/v1.10.17/done-hub.exe",
            "hash": "de4da11909a95bb221b7c2e359ba1c1834c31091d1cd2268d2113963553587ea"
        }
    },
    "bin": "done-hub.exe",
    "pre_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install"
    ],
    "post_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Stop-Process"
    ],
    "post_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "A-Get-VersionFromGithubAPI"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/deanxv/done-hub/releases/download/v$version/done-hub.exe"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
