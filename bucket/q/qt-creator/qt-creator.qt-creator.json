{
    "version": "19.0.0-beta1",
    "description": "用于 Qt 框架开发的集成开发环境 (IDE) | IDE for development with the Qt framework",
    "homepage": "https://doc.qt.io/qtcreator/index.html",
    "license": {
        "identifier": "Unknown"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/qt-creator/qt-creator/releases/download/v19.0.0-beta1/qtcreator-windows-x64-msvc-19.0.0-beta1.7z",
            "hash": "796534676d86eb4cc8b6ba9d99a72fb68d90f8b19f17e600c2c0292b9c1fb832"
        }
    },
    "extract_to": "app",
    "bin": "app\\bin\\qtcreator.exe",
    "shortcuts": [
        [
            "app\\bin\\qtcreator.exe",
            "Qt Creator"
        ]
    ],
    "pre_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install",
        "A-New-LinkDirectory \"$env:AppData\\QtProject\""
    ],
    "post_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Stop-Process",
        "A-Remove-Link"
    ],
    "post_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "A-Get-VersionFromGithubAPI"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/qt-creator/qt-creator/releases/download/v$version/qtcreator-windows-x64-msvc-$version.7z"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
