{
    "version": "1.1.6+23",
    "description": "一个 Flutter LLM 聊天客户端。 | A Flutter LLM Chat Client.",
    "homepage": "https://kelivo.psycheas.top/",
    "license": {
        "identifier": "AGPL-3.0-only"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/Chevey339/kelivo/releases/download/v1.1.6/Kelivo_windows_1.1.6+23_setup.exe",
            "hash": "45cbc9719f227835063b02b1c2a4855fd2b97c42d5db54754f567c6c17d465c0"
        }
    },
    "shortcuts": [
        [
            "app\\kelivo.exe",
            "Kelivo"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install",
        "A-Require-Admin",
        "A-New-LinkDirectory \"$env:AppData\\com.psyche\\kelivo\", \"$env:LocalAppData\\com.psyche\\kelivo\"",
        "A-Install-Inno"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Require-Admin",
        "A-Stop-Process",
        "A-Remove-Link",
        "A-Uninstall-Inno"
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "$InstallerInfo = A-Get-InstallerInfoFromWinget 'Psyche.Kelivo' 'exe'",
            "\"ver:$($InstallerInfo.PackageVersion);\"",
            "\"x64:$($InstallerInfo.x64_machine.InstallerUrl);\"",
            "\"x86:$($InstallerInfo.x86_machine.InstallerUrl);\"",
            "\"arm64:$($InstallerInfo.arm64_machine.InstallerUrl);\""
        ],
        "regex": "ver:(?<version>[^;]+)(?:; x64:(?<x64>[^;]*))?(?:; x86:(?<x86>[^;]*))?(?:; arm64:(?<arm64>[^;]*))?"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "$matchX64",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.x64_machine.InstallerSha256"
                }
            }
        }
    }
}
