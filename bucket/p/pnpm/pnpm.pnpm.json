            "hash": "3e4285f1ee6e85e0521c22c63b53bb5deefe07ae83c6a6862d3f1818d5180b44"
        },
        "arm64": {
            "url": "https://github.com/pnpm/pnpm/releases/download/v10.26.2/pnpm-win-arm64.exe#/pnpm.exe",
            "hash": "7f0fef983ab98010b7fd4aceb24a89aa5c52ca6f48cc6291111815baa2da0e61"
