{
    "version": "0.83.0.0",
    "description": "一个适用于 Windows 和 Unix 平台的 SSH 和 Telnet 的免费实现，以及一个 xterm 终端模拟器 | A free implementation of SSH and Telnet for Windows and Unix platforms, along with an xterm terminal emulator",
    "homepage": "https://www.chiark.greenend.org.uk/",
    "license": {
        "identifier": "MIT"
    },
    "architecture": {
        "64bit": {
            "url": "https://the.earth.li/~sgtatham/putty/0.83/w64/putty-64bit-0.83-installer.msi#/dl.msi_",
            "hash": "aa8e5036d973688f1e8622fbe9ab22e037346e0def0197bf5e7cdf37da4e223d"
        },
        "arm64": {
            "url": "https://the.earth.li/~sgtatham/putty/0.83/wa64/putty-arm64-0.83-installer.msi#/dl.msi_",
            "hash": "591ef6bd9b200772875d152200b321366129b746f163666742fa5e201479bb53"
        }
    },
    "pre_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install",
        "A-Require-Admin",
        "A-Install-Msi"
    ],
    "post_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Require-Admin",
        "A-Stop-Process",
        "A-Uninstall-Msi"
    ],
    "post_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "$res, $out = A-Get-InstallerInfoFromWinget 'PuTTY.PuTTY' 'msi'",
            "$out"
        ],
        "regex": "(?:.*ver:(?<version>[^;]+))(?:.*x64:(?<x64>[^;]+))?(?:.*x86:(?<x86>[^;]+))?(?:.*arm64:(?<arm64>[^;]+))?"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "$matchX64#/dl.msi_",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.x64_machine.InstallerSha256"
                }
            },
            "arm64": {
                "url": "$matchArm64#/dl.msi_",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.arm64_machine.InstallerSha256"
                }
            }
        }
    }
}
