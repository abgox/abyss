{
    "version": "2.7.9",
    "description": "一个围绕模糊查找工具 fzf 的 PowerShell 包装器 | A PowerShell wrapper around the fuzzy finder fzf",
    "homepage": "https://github.com/kelleyma49/PSFzf",
    "license": {
        "identifier": "MIT",
        "url": "https://github.com/kelleyma49/PSFzf/blob/master/LICENSE"
    },
    "suggest": {
        "fzf": [
            "fzf",
            "abyss/junegunn.fzf"
        ]
    },
    "architecture": {
        "64bit": {
            "url": "https://cdn.powershellgallery.com/packages/psfzf.2.7.9.nupkg",
            "hash": "5b20d267048512b5acc6d51e74d7f660cfd8aacb607260a063a6ac835931fb57"
        },
        "arm64": {
            "url": "https://cdn.powershellgallery.com/packages/psfzf.2.7.9.nupkg",
            "hash": "5b20d267048512b5acc6d51e74d7f660cfd8aacb607260a063a6ac835931fb57"
        }
    },
    "psmodule": {
        "name": "PSFzf"
    },
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Stop-Process"
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "url": "https://www.powershellgallery.com/packages/psfzf",
        "regex": "<h2>([\\d.]+)</h2>"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://cdn.powershellgallery.com/packages/psfzf.$version.nupkg"
            },
            "arm64": {
                "url": "https://cdn.powershellgallery.com/packages/psfzf.$version.nupkg"
            }
        }
    }
}
