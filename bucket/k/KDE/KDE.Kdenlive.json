{
    "version": "25.12.2",
    "description": "免费开源的视频编辑器 | Free and Open Source Video Editor",
    "homepage": "https://kdenlive.org/",
    "license": {
        "identifier": "GPL-3.0-only",
        "url": "https://invent.kde.org/multimedia/kdenlive/-/blob/HEAD/COPYING"
    },
    "architecture": {
        "64bit": {
            "url": "https://download.kde.org/stable/kdenlive/25.12/windows/kdenlive-25.12.2.exe",
            "hash": "a9a3c693edf66f730eff3332b95faf0d88daf610027678cddf5720402d58e373"
        }
    },
    "pre_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install",
        "A-Require-Admin",
        "A-New-LinkFile \"$env:LocalAppData\\kdenliverc\"",
        "A-New-LinkDirectory \"$env:AppData\\kdenlive\"",
        "A-Install-App -Uninstaller \"$env:ProgramFiles\\kdenlive\\uninstall.exe\""
    ],
    "post_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Require-Admin",
        "A-Stop-Process \"$env:ProgramFiles\\kdenlive\"",
        "A-Remove-Link",
        "A-Uninstall-App",
        "A-Remove-TempData \"$env:LocalAppData\\kdenlive\""
    ],
    "post_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "$res, $out = A-Get-InstallerInfoFromWinget 'KDE.Kdenlive' 'exe'",
            "$out"
        ],
        "regex": "(?:.*ver:(?<version>[^;]+))(?:.*x64:(?<x64>[^;]+))?(?:.*x86:(?<x86>[^;]+))?(?:.*arm64:(?<arm64>[^;]+))?"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "$matchX64",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.x64_user.InstallerSha256"
                }
            }
        }
    }
}
