    "architecture": {
        "64bit": {
            "url": "https://github.com/Kindness-Kismet/Stelliberty/releases/download/v1.1.182/Stelliberty-v1.1.182-windows-x64-setup.exe",
            "hash": "97a538cda7381445d58aaf0d82901ffd56befa5d45927cc69a04a1d9db641e1d"
        },
        "arm64": {
            "url": "https://github.com/Kindness-Kismet/Stelliberty/releases/download/v1.1.182/Stelliberty-v1.1.182-windows-arm64-setup.exe",
            "hash": "c881dd7c92480f2b8c0827e03c5ddfda517e5f11576194a59fda971f6c870af3"
        }
    },
    "shortcuts": [
        [
            "app\\stelliberty.exe",
            "Stelliberty"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Install",
        "A-Require-Admin",
        "A-New-LinkDirectory \"$env:AppData\\stelliberty\"",
        "A-Install-Exe inno -Uninstaller 'app\\uninstall\\unins000.exe'"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Uninstall",
        "A-Uninstall-ExeManually 'app'",
        "A-Remove-Link"
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". \"$pwd\\bin\\utils.ps1\"",
            "A-Get-VersionFromGithubAPI"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/Kindness-Kismet/Stelliberty/releases/download/v$version/Stelliberty-v$version-windows-x64-setup.exe"
            },
            "arm64": {
                "url": "https://github.com/Kindness-Kismet/Stelliberty/releases/download/v$version/Stelliberty-v$version-windows-arm64-setup.exe"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
