    "architecture": {
        "64bit": {
            "url": "https://github.com/Kindness-Kismet/Stelliberty/releases/download/v1.2.010/Stelliberty-v1.2.010-windows-x64-setup.exe",
            "hash": "0a6482a422c0559580deb6ce752a4ae6f4c0ef666f59ab7288cb798dc9adbcee"
        },
        "arm64": {
            "url": "https://github.com/Kindness-Kismet/Stelliberty/releases/download/v1.2.010/Stelliberty-v1.2.010-windows-arm64-setup.exe",
            "hash": "5fadf9ba94c8adc387807afa1a9111355d18fb9249d349fc7485e806d6617982"
        }
    },
    "shortcuts": [
        [
            "app\\stelliberty.exe",
            "Stelliberty"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install",
        "A-Require-Admin",
        "A-New-LinkDirectory \"$env:AppData\\stelliberty\"",
        "A-Install-Inno -Uninstaller 'app\\uninstall\\unins000.exe'"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Uninstall-Manually 'app'",
        "A-Remove-Link"
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "A-Get-VersionFromGithubAPI"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/Kindness-Kismet/Stelliberty/releases/download/v$version/Stelliberty-v$version-windows-x64-setup.exe"
            },
            "arm64": {
                "url": "https://github.com/Kindness-Kismet/Stelliberty/releases/download/v$version/Stelliberty-v$version-windows-arm64-setup.exe"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
