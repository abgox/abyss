{
    "version": "1.2.12.1506",
    "description": "简单且强大的代理工具，支持 clash/sing-box 的路由规则 | Simple & Powerful proxy utility, Support routing rules for clash/sing-box",
    "homepage": "https://github.com/KaringX/karing",
    "license": {
        "identifier": "Unknown"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/KaringX/karing/releases/download/v1.2.12.1506/karing_1.2.12.1506_windows_x64.exe",
            "hash": "51902d8eb43dfe62a133c324452ddd722429512684812942cea09f49072d921c"
        }
    },
    "extract_to": "app",
    "innosetup": true,
    "shortcuts": [
        [
            "app\\karing.exe",
            "Karing"
        ]
    ],
    "persist": [
        [
            "app\\profiles",
            "AppData\\Roaming\\karing\\karing"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Stop-Process"
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "$InstallerInfo, $out = A-Get-InstallerInfoFromWinget 'KaringX.Karing' 'exe'",
            "$out"
        ],
        "regex": "(?:.*ver:(?<version>[^;]+))(?:.*x64:(?<x64>[^;]+))?(?:.*x86:(?<x86>[^;]+))?(?:.*arm64:(?<arm64>[^;]+))?"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "$matchX64",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.x64_machine.InstallerSha256"
                }
            }
        }
    }
}
