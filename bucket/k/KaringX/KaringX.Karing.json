{
    "version": "1.2.14.1701",
    "description": "简单且强大的代理工具，支持 clash/sing-box 的路由规则 | Simple & Powerful proxy utility, Support routing rules for clash/sing-box",
    "homepage": "https://github.com/KaringX/karing",
    "license": {
        "identifier": "Unknown"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/KaringX/karing/releases/download/v1.2.14.1701/karing_1.2.14.1701_windows_x64.exe",
            "hash": "a810f536f74e55dc2bb8aa5f0321663c6dede93439ab49583db117c31bbaca58"
        }
    },
    "extract_to": "app",
    "innosetup": true,
    "shortcuts": [
        [
            "app\\karing.exe",
            "Karing"
        ]
    ],
    "persist": [
        [
            "app\\profiles",
            "AppData\\Roaming\\karing\\karing"
        ]
    ],
    "pre_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install"
    ],
    "post_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Stop-Process"
    ],
    "post_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "$res, $out = A-Get-InstallerInfoFromWinget 'KaringX.Karing' 'exe'",
            "$out"
        ],
        "regex": "(?:.*ver:(?<version>[^;]+))(?:.*x64:(?<x64>[^;]+))?(?:.*x86:(?<x86>[^;]+))?(?:.*arm64:(?<arm64>[^;]+))?"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "$matchX64",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.x64_machine.InstallerSha256"
                }
            }
        }
    }
}
