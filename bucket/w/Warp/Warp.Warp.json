{
    "version": "0.2025.12.17.17.17.stable_02",
    "description": "一款内置 AI 和开发团队知识的智能终端 | The intelligent terminal with AI and your dev team's knowledge built-in.",
    "homepage": "https://www.warp.dev/",
    "license": {
        "identifier": "Unknown",
        "url": "https://www.warp.dev/terms-of-service"
    },
    "architecture": {
        "64bit": {
            "url": "https://app.warp.dev/download/windows?version=v0.2025.12.17.17.17.stable_02&arch=x86_64#/dl.exe",
            "hash": "fe27f2611770eadca7dd55a7c59a9e798021f9185351d5706bcb39fe89001386"
        },
        "arm64": {
            "url": "https://app.warp.dev/download/windows?version=v0.2025.12.17.17.17.stable_02&arch=arm64#/dl.exe",
            "hash": "d5e8b0d61c78a713e78b77acfe61256024620b18d57861395d16d9a7371e7023"
        }
    },
    "env_add_path": "app\\bin",
    "shortcuts": [
        [
            "app\\warp.exe",
            "Warp"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install",
        "A-New-LinkDirectory \"$env:LocalAppData\\warp\\Warp\"",
        "A-Install-App inno"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Stop-Process",
        "A-Remove-Link",
        "A-Uninstall-App"
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "$InstallerInfo = A-Get-InstallerInfoFromWinget 'Warp.Warp' ''",
            "\"ver:$($InstallerInfo.PackageVersion);\"",
            "\"x64:$($InstallerInfo.x64_user.InstallerUrl);\"",
            "\"x86:$($InstallerInfo.x86_user.InstallerUrl);\"",
            "\"arm64:$($InstallerInfo.arm64_user.InstallerUrl);\""
        ],
        "regex": "ver:(?<version>[^;]+)(?:; x64:(?<x64>[^;]*))?(?:; x86:(?<x86>[^;]*))?(?:; arm64:(?<arm64>[^;]*))?"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "$matchX64#/dl.exe",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.x64_user.InstallerSha256"
                }
            },
            "arm64": {
                "url": "$matchArm64#/dl.exe",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.arm64_user.InstallerSha256"
                }
            }
        }
    }
}
