{
    "version": "0.2026.01.14.08.15.stable_04",
    "description": "一款内置 AI 和开发团队知识的智能终端 | The intelligent terminal with AI and your dev team's knowledge built-in.",
    "homepage": "https://www.warp.dev/",
    "license": {
        "identifier": "Unknown",
        "url": "https://www.warp.dev/terms-of-service"
    },
    "architecture": {
        "64bit": {
            "url": "https://app.warp.dev/download/windows?version=v0.2026.01.14.08.15.stable_04&arch=x86_64#/dl.exe",
            "hash": "ccf10f01b4b39367003a3b4bda0e680f423c703d68e0b2394fbfe53cfe6ead46"
        },
        "arm64": {
            "url": "https://app.warp.dev/download/windows?version=v0.2026.01.14.08.15.stable_04&arch=arm64#/dl.exe",
            "hash": "50ca414e7c5659558649f2c6e6c696d8b547a5e91b11dabc620dc80776455e11"
        }
    },
    "env_add_path": "app\\bin",
    "commands": "warp",
    "shortcuts": [
        [
            "app\\warp.exe",
            "Warp"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install",
        "A-New-LinkDirectory \"$env:LocalAppData\\warp\\Warp\"",
        "A-Install-Inno"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Stop-Process",
        "A-Remove-Link",
        "A-Uninstall-Inno"
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "$InstallerInfo, $out = A-Get-InstallerInfoFromWinget 'Warp.Warp' ''",
            "$out"
        ],
        "regex": "(?:.*ver:(?<version>[^;]+))(?:.*x64:(?<x64>[^;]+))?(?:.*x86:(?<x86>[^;]+))?(?:.*arm64:(?<arm64>[^;]+))?"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "$matchX64#/dl.exe",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.x64_user.InstallerSha256"
                }
            },
            "arm64": {
                "url": "$matchArm64#/dl.exe",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.arm64_user.InstallerSha256"
                }
            }
        }
    }
}
