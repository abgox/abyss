{
    "version": "1.2.3",
    "description": "一款现代、灵活的时间跟踪应用程序，用于记录和管理个人工作时间。A modern, flexible time tracking app for capturing and managing personal working hours.",
    "homepage": "https://github.com/WINBIGFOX/TimeScribe",
    "license": {
        "identifier": "GPL-3.0-only",
        "url": "https://github.com/WINBIGFOX/TimeScribe/blob/main/LICENSE"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/WINBIGFOX/TimeScribe/releases/download/v1.2.3/TimeScribe-1.2.3-setup.exe",
            "hash": "sha512:5f17a7f44810b949c91405ee5a67db66457523b2ce26c43aaf3073548842ca73a788e9ee37956bc9c25f8b802af6ea49cd70b5273d26765737e0d146fd4fab1c"
        }
    },
    "shortcuts": [
        [
            "timescribe.exe",
            "TimeScribe"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Install",
        "A-Install-Exe -Uninstaller 'Uninstall timescribe.exe'",
        "A-New-LinkDirectory @(\"$env:AppData\\timescribe\")"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($install.bucket) { $install.bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Uninstall",
        "A-Stop-Process",
        "A-Remove-Link",
        "A-Uninstall-Exe"
    ],
    "post_uninstall": [
        "$bucket = if ($install.bucket) { $install.bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "url": "https://github.com/WINBIGFOX/TimeScribe/releases/latest",
        "regex": "/tree/(?:v|V)?([^\"?]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/WINBIGFOX/TimeScribe/releases/download/v$version/TimeScribe-$version-setup.exe",
                "hash": {
                    "url": "$baseurl/latest.yml",
                    "regex": "sha512:\\s+$base64"
                }
            }
        }
    }
}
