{
    "version": "2.9.6",
    "description": "一款无需任何配置即可使用的分页器 | A pager designed to work without any configuration",
    "homepage": "https://github.com/walles/moor",
    "license": {
        "identifier": "Unknown",
        "url": "https://github.com/walles/moor/blob/master/LICENSE"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/walles/moor/releases/download/v2.9.6/moor-v2.9.6-windows-amd64.exe#/moor.exe",
            "hash": "064640acd8b38aed6e4419cb8bad34f2c99b725a8aed2ec0c654241e8f616a48"
        }
    },
    "env_set": {
        "PAGER": "$dir\\moor.exe"
    },
    "bin": "moor.exe",
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Install"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Uninstall",
        "A-Stop-Process"
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". \"$pwd\\bin\\utils.ps1\"",
            "A-Get-VersionFromGithubAPI"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/walles/moor/releases/download/v$version/moor-v$version-windows-amd64.exe#/moor.exe"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
