{
    "version": "2.11.0",
    "description": "一款无需任何配置即可使用的分页器 | A pager designed to work without any configuration",
    "homepage": "https://github.com/walles/moor",
    "license": {
        "identifier": "Unknown"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/walles/moor/releases/download/v2.11.0/moor-v2.11.0-windows-amd64.exe#/moor.exe",
            "hash": "a19df157dda4bf46afbfb5a9340ff7e289fa542322ea2bb529aa18c659410aec"
        }
    },
    "env_set": {
        "PAGER": "$dir\\moor.exe"
    },
    "bin": "moor.exe",
    "pre_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install"
    ],
    "post_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Stop-Process"
    ],
    "post_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "A-Get-VersionFromGithubAPI"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/walles/moor/releases/download/v$version/moor-v$version-windows-amd64.exe#/moor.exe"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
