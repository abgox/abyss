{
    "version": "2.9.4",
    "description": "一款无需任何配置即可使用的分页器 | A pager designed to work without any configuration",
    "homepage": "https://github.com/walles/moor",
    "license": {
        "identifier": "Unknown",
        "url": "https://github.com/walles/moor/blob/master/LICENSE"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/walles/moor/releases/download/v2.9.4/moor-v2.9.4-windows-amd64.exe#/moor.exe",
            "hash": "6e292b212e023cc79010d6b6ae38f8064d072b529dff2c3953a519a977a8e4a7"
        }
    },
    "env_set": {
        "PAGER": "$dir\\moor.exe"
    },
    "bin": "moor.exe",
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Install"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Uninstall",
        "A-Stop-Process"
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". \"$pwd\\bin\\utils.ps1\"",
            "A-Get-VersionFromGithubAPI"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/walles/moor/releases/download/v$version/moor-v$version-windows-amd64.exe#/moor.exe"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
