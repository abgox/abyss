{
    "version": "1.73.0",
    "description": "一个用于在不同云存储提供商之间同步文件和目录的命令行程序 | A command-line program to sync files and directories to and from different cloud storage providers",
    "homepage": "https://rclone.org/",
    "license": {
        "identifier": "MIT"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/rclone/rclone/releases/download/v1.73.0/rclone-v1.73.0-windows-amd64.zip",
            "hash": "14e1c40f34ec18532e832c228231338bd182817af6f6529a402474c69acabe0b",
            "extract_dir": "rclone-v1.73.0-windows-amd64"
        },
        "arm64": {
            "url": "https://github.com/rclone/rclone/releases/download/v1.73.0/rclone-v1.73.0-windows-arm64.zip",
            "hash": "fd1fdef477664829903e11e32e07839446d238ae133e3edef516f59ec7a9cd50",
            "extract_dir": "rclone-v1.73.0-windows-arm64"
        }
    },
    "extract_to": "app",
    "bin": "app\\rclone.exe",
    "pre_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install",
        "A-New-LinkDirectory \"$env:AppData\\rclone\""
    ],
    "post_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Stop-Process",
        "A-Remove-Link"
    ],
    "post_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "A-Get-VersionFromGithubAPI"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/rclone/rclone/releases/download/v$version/rclone-v$version-windows-amd64.zip",
                "extract_dir": "rclone-v$version-windows-amd64"
            },
            "arm64": {
                "url": "https://github.com/rclone/rclone/releases/download/v$version/rclone-v$version-windows-arm64.zip",
                "extract_dir": "rclone-v$version-windows-arm64"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
