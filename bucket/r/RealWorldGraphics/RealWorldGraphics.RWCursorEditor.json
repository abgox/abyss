        }
    },
    "extract_to": "app",
    "shortcuts": [
        [
            "app\\RWCursorEditor.exe",
            "RealWorld Cursor Editor"
        ]
    ],
    "persist": [
        [
            "app\\AppData",
            "AppData\\Roaming\\RealWorld\\RWCursorEditor"
        ]
