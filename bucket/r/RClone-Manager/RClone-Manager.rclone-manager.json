{
    "version": "0.1.9",
    "description": "一款功能强大的跨平台图形用户界面，可轻松且美观地管理 Rclone remotes | A powerful, cross-platform GUI for managing Rclone remotes with style and ease.",
    "homepage": "https://zarestia-dev.github.io/rclone-manager/",
    "license": {
        "identifier": "GPL-3.0-only",
        "url": "https://github.com/Zarestia-Dev/rclone-manager/blob/master/LICENSE"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/Zarestia-Dev/rclone-manager/releases/download/v0.1.9/RClone.Manager_0.1.9_x64-setup.exe",
            "hash": "ddaff1cec4284f8a827439bf78e7fb734ef39379650ca4d8414b4d38cf0fda40"
        },
        "arm64": {
            "url": "https://github.com/Zarestia-Dev/rclone-manager/releases/download/v0.1.9/RClone.Manager_0.1.9_arm64-setup.exe",
            "hash": "dbb4b41fd141d3af2b9eaf969a1ef7366d71d20efd656935f957ae5bab8eb77f"
        }
    },
    "shortcuts": [
        [
            "app\\rclone-manager.exe",
            "RClone Manager"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install",
        "A-New-LinkDirectory \"$env:AppData\\com.rclone.manager\"",
        "A-Install-App -Uninstaller 'app\\uninstall.exe'"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Stop-Process @() 'rclone'",
        "A-Remove-Link",
        "A-Uninstall-App",
        "A-Remove-TempData \"$env:LocalAppData\\com.rclone.manager\""
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "A-Get-VersionFromGithubAPI"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/Zarestia-Dev/rclone-manager/releases/download/v$version/RClone.Manager_$version_x64-setup.exe"
            },
            "arm64": {
                "url": "https://github.com/Zarestia-Dev/rclone-manager/releases/download/v$version/RClone.Manager_$version_arm64-setup.exe"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
