{
    "version": "0.1.8",
    "description": "一款功能强大的跨平台图形用户界面，可轻松且美观地管理 Rclone remotes | A powerful, cross-platform GUI for managing Rclone remotes with style and ease.",
    "homepage": "https://zarestia-dev.github.io/rclone-manager/",
    "license": {
        "identifier": "GPL-3.0-only",
        "url": "https://github.com/Zarestia-Dev/rclone-manager/blob/master/LICENSE"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/Zarestia-Dev/rclone-manager/releases/download/v0.1.8/RClone.Manager_0.1.8_x64-setup.exe",
            "hash": "54e5ad7a7d5068ed1f0a50d7689569e016cb9a8a1313ceb34f440721d315db6e"
        },
        "arm64": {
            "url": "https://github.com/Zarestia-Dev/rclone-manager/releases/download/v0.1.8/RClone.Manager_0.1.8_arm64-setup.exe",
            "hash": "89668783f5ec012c9fe35c2e571c05ef7f91f90056acfa411f080406f19abcd3"
        }
    },
    "shortcuts": [
        [
            "app\\rclone-manager.exe",
            "RClone Manager"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Install",
        "A-New-LinkDirectory @(\"$env:AppData\\com.rclone.manager\")",
        "A-Install-Exe -Uninstaller 'app\\uninstall.exe'"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Uninstall",
        "A-Stop-Process @() @('rclone')",
        "A-Remove-Link",
        "A-Uninstall-Exe",
        "A-Remove-TempData @(\"$env:LocalAppData\\com.rclone.manager\")"
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". \"$pwd\\bin\\utils.ps1\"",
            "A-Get-VersionFromGithubAPI"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/Zarestia-Dev/rclone-manager/releases/download/v$version/RClone.Manager_$version_x64-setup.exe"
            },
            "arm64": {
                "url": "https://github.com/Zarestia-Dev/rclone-manager/releases/download/v$version/RClone.Manager_$version_arm64-setup.exe"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
