{
    "version": "0.54.0",
    "description": "一个可插拔的多语言版本管理器 | A pluggable multi-language version manager.",
    "homepage": "https://moonrepo.dev/proto",
    "license": {
        "identifier": "MIT",
        "url": "https://github.com/moonrepo/proto/blob/master/LICENSE"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/moonrepo/proto/releases/download/v0.54.0/proto_cli-x86_64-pc-windows-msvc.zip",
            "hash": "c6c4a797be308a65099e544fb620c69a7acacaea60c0462a1f1593d882f1a023"
        }
    },
    "extract_to": "app",
    "env_add_path": "$env:UserProfile\\.proto\\shims",
    "bin": "app\\proto.exe",
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Install",
        "A-New-LinkDirectory @(\"$env:UserProfile\\.proto\")"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($install.bucket) { $install.bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Uninstall",
        "A-Stop-Process",
        "A-Remove-Link"
    ],
    "post_uninstall": [
        "$bucket = if ($install.bucket) { $install.bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "url": "https://github.com/moonrepo/proto/releases/latest",
        "regex": "/tree/(?:v|V)?([^\"?]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/moonrepo/proto/releases/download/v$version/proto_cli-x86_64-pc-windows-msvc.zip"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
