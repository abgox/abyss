{
    "version": "25.0.1.8",
    "description": "微软构建的 OpenJDK 是一种全新的免费长期支持发行版，也是微软与 Java 生态系统开展协作和贡献的新方式 | The Microsoft Build of OpenJDK is a new no-cost long-term supported distribution and Microsoft's new way to collaborate and contribute to the Java ecosystem",
    "homepage": "https://www.microsoft.com/openjdk",
    "license": {
        "identifier": "GPL2 with Classpath Exception",
        "url": "https://openjdk.java.net/legal/gplv2+ce.html"
    },
    "architecture": {
        "64bit": {
            "url": "https://aka.ms/download-jdk/microsoft-jdk-25.0.1-windows-x64.msi#winget#/dl.msi",
            "hash": "5ce322c00d8d56b99eeaec7abad6328f2b95f4d22e23558fc23b87464ff27597"
        },
        "arm64": {
            "url": "https://aka.ms/download-jdk/microsoft-jdk-25.0.1-windows-aarch64.msi#winget#/dl.msi",
            "hash": "80a095f4931da10964c6a7d3397c1ef944d770ee1e93bd44c6ee2f5a11fa31c4"
        }
    },
    "extract_dir": "PFiles64\\Microsoft\\jdk-25.0.1.8-hotspot",
    "extract_to": "app",
    "env_set": {
        "JAVA_HOME": "$dir\\app"
    },
    "env_add_path": "app\\bin",
    "commands": [
        "java",
        "jabswitch"
    ],
    "pre_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install"
    ],
    "post_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall"
    ],
    "post_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "$InstallerInfo, $out = A-Get-InstallerInfoFromWinget 'Microsoft.OpenJDK.25' 'msi'",
            "$out"
        ],
        "regex": "(?:.*ver:(?<version>[^;]+))(?:.*x64:(?<x64>[^;]+))?(?:.*x86:(?<x86>[^;]+))?(?:.*arm64:(?<arm64>[^;]+))?"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "$matchX64#/dl.msi",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.x64_machine.InstallerSha256"
                }
            },
            "arm64": {
                "url": "$matchArm64#/dl.msi",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.arm64_machine.InstallerSha256"
                }
            }
        },
        "extract_dir": "PFiles64\\Microsoft\\jdk-$version-hotspot"
    }
}
