{
    "version": "139.0.3405.13",
    "description": "(Dev 版本) 微软 Edge 浏览器 - 一款由微软创建和开发的跨平台网络浏览器。(Dev version) Microsoft Edge - A cross-platform web browser created and developed by Microsoft.",
    "homepage": "https://www.microsoft.com/edge",
    "license": {
        "identifier": "Unknown",
        "url": "https://www.microsoft.com/servicesagreement"
    },
    "notes": [
        "The installation directory: C:\\Program Files (x86)\\Microsoft\\Edge Dev",
        "Scoop only manages data, installation, update, and uninstallation operations."
    ],
    "notes-cn": [
        "实际安装目录: C:\\Program Files (x86)\\Microsoft\\Edge Dev",
        "Scoop 只管理数据(通过 Link)，安装、更新、卸载操作"
    ],
    "architecture": {
        "64bit": {
            "url": "https://msedge.sf.dl.delivery.mp.microsoft.com/filestreamingservice/files/592a622c-990a-4e6b-8661-12f5871f5750/MicrosoftEdgeDevEnterpriseX64.msi#/dl.exe",
            "hash": "f5e5b86b59045e1996733bc390e4b247bff92efcdd4574b2e7c30f25201ce941"
        },
        "arm64": {
            "url": "https://msedge.sf.dl.delivery.mp.microsoft.com/filestreamingservice/files/ee4b9c5b-0316-44b9-a797-2a40eeaf4cd5/MicrosoftEdgeDevEnterpriseARM64.msi#/dl.exe",
            "hash": "8d090a09957720a286460a20ebebdcd6fa013519442655013316292c1969211f"
        }
    },
    "shortcuts": [
        [
            "C:\\Program Files (x86)\\Microsoft\\Edge Dev\\Application\\msedge.exe",
            "Microsoft Edge Dev"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PreInstall",
        "A-Require-Admin",
        "A-Install-Exe -Installer 'C:\\Windows\\System32\\msiexec.exe' -ArgumentList @('/i', \"$dir\\$fname\", '/passive')",
        "A-New-LinkDirectory @(\"$env:LocalAppData\\Microsoft\\Edge Dev\\User Data\")"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PostInstall"
    ],
    "pre_uninstall": [
        "$bucket = if ($install.bucket) { $install.bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PreUninstall",
        "A-Require-Admin",
        "$ProductCode = A-Get-ProductCode '^Microsoft Edge Dev$'",
        "A-Uninstall-Exe -Uninstaller 'C:\\Windows\\System32\\msiexec.exe' -ArgumentList @('/x', $ProductCode, '/passive') -FailureFile 'C:\\Program Files (x86)\\Microsoft\\Edge Dev\\Application\\msedge.exe'",
        "A-Remove-Link"
    ],
    "post_uninstall": [
        "$bucket = if ($install.bucket) { $install.bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PostUninstall"
    ],
    "checkver": {
        "script": [
            ". \"$pwd\\bin\\utils.ps1\"",
            "$InstallerInfo = A-Get-InstallerInfoFromWinget 'Microsoft.Edge.Dev' 'msi'",
            "\"ver:$($InstallerInfo.PackageVersion);\"",
            "\"x64:$($InstallerInfo.x64_machine.InstallerUrl);\"",
            "\"x86:$($InstallerInfo.x86_machine.InstallerUrl);\"",
            "\"arm64:$($InstallerInfo.arm64_machine.InstallerUrl);\""
        ],
        "regex": "ver:(?<version>[^;]+)(?:; x64:(?<x64>[^;]*))?(?:; x86:(?<x86>[^;]*))?(?:; arm64:(?<arm64>[^;]*))?"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "$matchX64#/dl.exe",
                "hash": {
                    "url": "bin/scoop-auto-check-update-temp-data.jsonc",
                    "jsonpath": "$.x64_machine.InstallerSha256"
                }
            },
            "arm64": {
                "url": "$matchArm64#/dl.exe",
                "hash": {
                    "url": "bin/scoop-auto-check-update-temp-data.jsonc",
                    "jsonpath": "$.arm64_machine.InstallerSha256"
                }
            }
        }
    }
}
