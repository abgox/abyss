{
    "version": "26.0.0.5436",
    "description": "适用于 Windows 的增强型终端，配备 X11 服务器、多标签 SSH 客户端、网络工具等诸多功能 | Enhanced terminal for Windows with X11 server, tabbed SSH client, network tools and much more",
    "homepage": "https://mobaxterm.mobatek.net/",
    "license": {
        "identifier": "Unknown",
        "url": "https://mobaxterm.mobatek.net/license.html"
    },
    "architecture": {
        "64bit": {
            "url": "https://download.mobatek.net/2602026012910130/MobaXterm_Installer_v26.0.zip",
            "hash": "a29dcc27bf874ad416bf00269db9ace546951cd49c7f9eb9b1837b08953d84a7"
        }
    },
    "extract_to": "app",
    "pre_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install",
        "A-Require-Admin",
        "A-New-LinkDirectory \"$env:AppData\\MobaXterm\"",
        "$msi = Get-ChildItem $dir '*.msi' -Recurse | Select-Object -First 1",
        "$fname = Join-Path 'app' $msi.Name",
        "A-Install-Msi"
    ],
    "post_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Require-Admin",
        "A-Stop-Process",
        "A-Remove-Link",
        "A-Uninstall-Msi"
    ],
    "post_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "$res, $out = A-Get-InstallerInfoFromWinget 'Mobatek.MobaXterm' 'zip'",
            "$out"
        ],
        "regex": "(?:.*ver:(?<version>[^;]+))(?:.*x64:(?<x64>[^;]+))?(?:.*x86:(?<x86>[^;]+))?(?:.*arm64:(?<arm64>[^;]+))?"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "$matchX86",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.x86_machine.InstallerSha256"
                }
            }
        }
    }
}
