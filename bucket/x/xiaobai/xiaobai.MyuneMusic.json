{
    "version": "0.7.2",
    "description": "一个基于 Flutter (Dart) 实现的简易本地音乐播放器 | A simple music player implemented using Flutter (Dart)",
    "homepage": "https://github.com/xiaobaimc/myune_music",
    "license": {
        "identifier": "Apache-2.0",
        "url": "https://github.com/xiaobaimc/myune_music/blob/master/LICENSE"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/xiaobaimc/myune_music/releases/download/v0.7.2/MyuneMusic_v0.7.2_windows-x64.zip",
            "hash": "b25e3c3da61795d08c8973d840b905ab71dc159f6a45d4c3245c17f84e3c9fe1"
        }
    },
    "extract_to": "app",
    "shortcuts": [
        [
            "app\\myune_music.exe",
            "MyuneMusic"
        ]
    ],
    "persist": [
        [
            "app\\list_data",
            "list_data"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Install",
        "A-New-LinkDirectory @(\"$env:AppData\\com.example\\myune_music\")"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Uninstall",
        "A-Stop-Process",
        "A-Remove-Link"
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "url": "https://github.com/xiaobaimc/myune_music",
        "script": [
            ". \"$pwd\\bin\\utils.ps1\"",
            "A-Get-VersionFromGithubApi"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/xiaobaimc/myune_music/releases/download/v$version/MyuneMusic_v$version_windows-x64.zip"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
