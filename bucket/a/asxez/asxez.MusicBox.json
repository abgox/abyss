{
    "version": "0.1.0-beta2",
    "description": "一款高颜值的本地音乐播放器。A pretty local music player.",
    "homepage": "https://github.com/asxez/MusicBox",
    "license": {
        "identifier": "MIT",
        "url": "https://github.com/asxez/MusicBox/blob/master/LICENSE"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/asxez/MusicBox/releases/download/0.1.0-beta2/musicbox-0.1.0-beta2.zip",
            "hash": "2c5da753699a010fa69bbb15fd5a573e8a924d14baa92fb35da52b5cbb4475fb"
        }
    },
    "extract_dir": "musicbox-0.1.0-beta2\\win-unpacked",
    "shortcuts": [
        [
            "MusicBox.exe",
            "MusicBox"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Install",
        "A-New-LinkDirectory @(\"$env:AppData\\musicbox\")"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($install.bucket) { $install.bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Uninstall",
        "A-Stop-Process",
        "A-Remove-Link"
    ],
    "post_uninstall": [
        "$bucket = if ($install.bucket) { $install.bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "url": "https://github.com/asxez/MusicBox/releases/latest",
        "regex": "/tree/(?:v|V)?([^\"?]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/asxez/MusicBox/releases/download/$version/musicbox-$version.zip"
            }
        },
        "extract_dir": "musicbox-$version\\win-unpacked"
    }
}
