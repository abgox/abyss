{
    "version": "2.3.23",
    "description": "一个 AI 代码编辑器 | The AI Code Editor.",
    "homepage": "https://www.cursor.com/",
    "license": {
        "identifier": "Freemium"
    },
    "architecture": {
        "64bit": {
            "url": "https://downloads.cursor.com/production/655ee705c6c7b6da1da481d0fdf13191d5e3e982/win32/x64/user-setup/CursorUserSetup-x64-2.3.23.exe",
            "hash": "561c7ac97d0aa4410d7584fb5fc741c74f1a857d397d804eeee4c30a1dc6cca2"
        },
        "arm64": {
            "url": "https://downloads.cursor.com/production/655ee705c6c7b6da1da481d0fdf13191d5e3e982/win32/arm64/user-setup/CursorUserSetup-arm64-2.3.23.exe",
            "hash": "fbc2818b4678bc55092fbe6ba05a1a30c4c4615541bcc1fa9a8539b85673a55b"
        }
    },
    "bin": "app\\Cursor.exe",
    "shortcuts": [
        [
            "app\\Cursor.exe",
            "Cursor"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Install",
        "A-New-LinkDirectory \"$env:AppData\\Cursor\",\"$env:UserProfile\\.cursor\"",
        "A-Install-Exe inno"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Uninstall",
        "A-Stop-Process",
        "A-Remove-Link",
        "A-Uninstall-Exe"
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". \"$pwd\\bin\\utils.ps1\"",
            "$InstallerInfo = A-Get-InstallerInfoFromWinget 'Anysphere.Cursor' 'exe'",
            "\"ver:$($InstallerInfo.PackageVersion);\"",
            "\"x64:$($InstallerInfo.x64_user.InstallerUrl);\"",
            "\"x86:$($InstallerInfo.x86_user.InstallerUrl);\"",
            "\"arm64:$($InstallerInfo.arm64_user.InstallerUrl);\""
        ],
        "regex": "ver:(?<version>[^;]+)(?:; x64:(?<x64>[^;]*))?(?:; x86:(?<x86>[^;]*))?(?:; arm64:(?<arm64>[^;]*))?"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "$matchX64",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.x64_user.InstallerSha256"
                }
            },
            "arm64": {
                "url": "$matchArm64",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.arm64_user.InstallerSha256"
                }
            }
        }
    }
}
