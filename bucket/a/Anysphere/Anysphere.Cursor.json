{
    "version": "2.4.27",
    "description": "一个 AI 代码编辑器 | The AI Code Editor",
    "homepage": "https://www.cursor.com/",
    "license": {
        "identifier": "Freemium"
    },
    "architecture": {
        "64bit": {
            "url": "https://downloads.cursor.com/production/4f2b772756b8f609e1354b3063de282ccbe7a69b/win32/x64/user-setup/CursorUserSetup-x64-2.4.27.exe",
            "hash": "ce051b7d50b430e8ce078ba24e5a2b67ec8655329e8e4a53fa3a22bd30c1d16e"
        },
        "arm64": {
            "url": "https://downloads.cursor.com/production/4f2b772756b8f609e1354b3063de282ccbe7a69b/win32/arm64/user-setup/CursorUserSetup-arm64-2.4.27.exe",
            "hash": "2e9e9ca4a5d35098d29c9b60e75d59ba9562206371975f3773fb01eaa9b3ad93"
        }
    },
    "bin": "app\\Cursor.exe",
    "shortcuts": [
        [
            "app\\Cursor.exe",
            "Cursor"
        ]
    ],
    "pre_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install",
        "A-New-LinkDirectory \"$env:AppData\\Cursor\",\"$home\\.cursor\"",
        "A-Install-Inno"
    ],
    "post_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Stop-Process",
        "A-Remove-Link",
        "A-Uninstall-Inno"
    ],
    "post_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "$res, $out = A-Get-InstallerInfoFromWinget 'Anysphere.Cursor' 'exe'",
            "$out"
        ],
        "regex": "(?:.*ver:(?<version>[^;]+))(?:.*x64:(?<x64>[^;]+))?(?:.*x86:(?<x86>[^;]+))?(?:.*arm64:(?<arm64>[^;]+))?"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "$matchX64",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.x64_user.InstallerSha256"
                }
            },
            "arm64": {
                "url": "$matchArm64",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.arm64_user.InstallerSha256"
                }
            }
        }
    }
}
