{
    "version": "2.5.20",
    "description": "一个 AI 代码编辑器 | The AI Code Editor",
    "homepage": "https://www.cursor.com/",
    "license": {
        "identifier": "Freemium"
    },
    "architecture": {
        "64bit": {
            "url": "https://downloads.cursor.com/production/511523af765daeb1fa69500ab0df5b6524424612/win32/x64/user-setup/CursorUserSetup-x64-2.5.20.exe",
            "hash": "55135951aa727a22182a15c21cf94d5874b9d3415c3d16789f7d4337fbbcc86b"
        },
        "arm64": {
            "url": "https://downloads.cursor.com/production/511523af765daeb1fa69500ab0df5b6524424612/win32/arm64/user-setup/CursorUserSetup-arm64-2.5.20.exe",
            "hash": "6138ff67ca406d0e2bdb2f77e23539e7d565b23ba42df6989374dc0d7bc481e1"
        }
    },
    "bin": "app\\Cursor.exe",
    "shortcuts": [
        [
            "app\\Cursor.exe",
            "Cursor"
        ]
    ],
    "pre_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install",
        "A-New-LinkDirectory \"$env:AppData\\Cursor\",\"$home\\.cursor\"",
        "A-Install-Inno"
    ],
    "post_install": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Stop-Process",
        "A-Remove-Link",
        "A-Uninstall-Inno"
    ],
    "post_uninstall": [
        "if (!$bucket) { $bucket = (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "$res, $out = A-Get-InstallerInfoFromWinget 'Anysphere.Cursor' 'exe'",
            "$out"
        ],
        "regex": "(?:.*ver:(?<version>[^;]+))(?:.*x64:(?<x64>[^;]+))?(?:.*x86:(?<x86>[^;]+))?(?:.*arm64:(?<arm64>[^;]+))?"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "$matchX64",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.x64_user.InstallerSha256"
                }
            },
            "arm64": {
                "url": "$matchArm64",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.arm64_user.InstallerSha256"
                }
            }
        }
    }
}
