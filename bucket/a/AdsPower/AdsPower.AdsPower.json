{
    "version": "7.7.18",
    "description": "一个指纹浏览器。A No anti-detection browser.",
    "homepage": "https://www.adspower.net/",
    "license": {
        "identifier": "Freemium",
        "url": "https://www.adspower.net/terms-of-use"
    },
    "architecture": {
        "64bit": {
            "url": "https://version.adspower.net/software/win64-global/7.7.18/AdsPower-Global-7.7.18-x64.exe",
            "hash": "f889c2fa26effdb7d93f31a8e45af86ddf1510f828f8759172bfbf079d24ed8b"
        }
    },
    "shortcuts": [
        [
            "AdsPower Global.exe",
            "AdsPower Browser"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PreInstall",
        "A-Require-Admin",
        "A-Install-Exe -Uninstaller 'Uninstall AdsPower Global.exe'",
        "A-New-LinkDirectory @(\"$env:AppData\\adspower_global\")"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PostInstall"
    ],
    "pre_uninstall": [
        "$bucket = if ($install.bucket) { $install.bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PreUninstall",
        "A-Require-Admin",
        "A-Stop-Process",
        "A-Remove-Link",
        "A-Uninstall-Exe",
        "A-Remove-TempData @(\"$env:LocalAppData\\adspower_global-updater\")"
    ],
    "post_uninstall": [
        "$bucket = if ($install.bucket) { $install.bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PostUninstall"
    ],
    "checkver": {
        "script": [
            ". \"$pwd\\bin\\utils.ps1\"",
            "$InstallerInfo = A-Get-InstallerInfoFromWinget 'AdsPower.AdsPower' 'exe'",
            "\"ver:$($InstallerInfo.PackageVersion);\"",
            "\"x64:$($InstallerInfo.x64_machine.InstallerUrl);\"",
            "\"x86:$($InstallerInfo.x86_machine.InstallerUrl);\"",
            "\"arm64:$($InstallerInfo.arm64_machine.InstallerUrl);\""
        ],
        "regex": "ver:(?<version>[^;]+)(?:; x64:(?<x64>[^;]*))?(?:; x86:(?<x86>[^;]*))?(?:; arm64:(?<arm64>[^;]*))?"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "$matchX64",
                "hash": {
                    "url": "bin/scoop-auto-check-update-temp-data.jsonc",
                    "jsonpath": "$.x64_machine.InstallerSha256"
                }
            }
        }
    }
}
