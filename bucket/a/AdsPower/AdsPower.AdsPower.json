{
    "version": "7.12.29",
    "description": "一个指纹浏览器 | A No anti-detection browser.",
    "homepage": "https://www.adspower.net/",
    "license": {
        "identifier": "Freemium",
        "url": "https://www.adspower.net/terms-of-use"
    },
    "architecture": {
        "64bit": {
            "url": "https://version.adspower.net/software/win64-global/7.12.29/AdsPower-Global-7.12.29-x64.exe",
            "hash": "ada9b93490bec374793f044df3c7117440afcaea239eb8d8b4821f1cfb232c23"
        }
    },
    "shortcuts": [
        [
            "app\\AdsPower Global.exe",
            "AdsPower Browser"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Install",
        "A-Require-Admin",
        "A-New-LinkDirectory \"$env:AppData\\adspower_global\"",
        "A-Install-Exe -Uninstaller 'app\\Uninstall AdsPower Global.exe'"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Start-Uninstall",
        "A-Require-Admin",
        "A-Stop-Process",
        "A-Remove-Link",
        "A-Uninstall-Exe",
        "A-Remove-TempData \"$env:LocalAppData\\adspower_global-updater\""
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". $bucketsdir\\$bucket\\script\\utils.ps1",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". $pwd\\script\\utils.ps1",
            "$InstallerInfo = A-Get-InstallerInfoFromWinget 'AdsPower.AdsPower' 'exe'",
            "\"ver:$($InstallerInfo.PackageVersion);\"",
            "\"x64:$($InstallerInfo.x64_machine.InstallerUrl);\"",
            "\"x86:$($InstallerInfo.x86_machine.InstallerUrl);\"",
            "\"arm64:$($InstallerInfo.arm64_machine.InstallerUrl);\""
        ],
        "regex": "ver:(?<version>[^;]+)(?:; x64:(?<x64>[^;]*))?(?:; x86:(?<x86>[^;]*))?(?:; arm64:(?<arm64>[^;]*))?"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "$matchX64",
                "hash": {
                    "url": "temp-autoupdate.json",
                    "jsonpath": "$.x64_machine.InstallerSha256"
                }
            }
        }
    }
}
