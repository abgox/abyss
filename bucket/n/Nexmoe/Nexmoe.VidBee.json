{
    "version": "1.1.0",
    "description": "适用于 YouTube、TikTok 及 1000 多个平台的免费开源视频下载器 | Free Open Source Video Downloader for YouTube, TikTok, and 1000+ Platforms",
    "homepage": "https://vidbee.org/",
    "license": {
        "identifier": "MIT"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/nexmoe/VidBee/releases/download/v1.1.0/vidbee-1.1.0-setup.exe",
            "hash": "2e72a33d624b630baa29737ef813c0acabaac6e706a88b1916b35b1c883f47c8"
        }
    },
    "shortcuts": [
        [
            "app\\vidbee.exe",
            "vidbee"
        ]
    ],
    "pre_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Install",
        "A-New-LinkDirectory \"$env:AppData\\vidbee\"",
        "A-Install-Exe -Uninstaller 'app\\Uninstall vidbee.exe'"
    ],
    "post_install": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Install"
    ],
    "pre_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-Uninstall",
        "A-Stop-Process",
        "A-Remove-Link",
        "A-Uninstall-Exe",
        "A-Remove-TempData \"$env:LocalAppData\\vidbee-updater\""
    ],
    "post_uninstall": [
        "$bucket = if ($bucket) { $bucket } else { (scoop config).'abgox-abyss-bucket-name' }",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Complete-Uninstall"
    ],
    "checkver": {
        "script": [
            ". \"$pwd\\bin\\utils.ps1\"",
            "A-Get-VersionFromGithubAPI"
        ],
        "regex": "(.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/nexmoe/VidBee/releases/download/v$version/vidbee-$version-setup.exe"
            }
        },
        "hash": {
            "mode": "github"
        }
    }
}
